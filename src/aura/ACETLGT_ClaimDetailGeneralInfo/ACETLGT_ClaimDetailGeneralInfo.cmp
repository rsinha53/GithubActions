<aura:component >
    <aura:attribute name="generalInfo" type="Map" default="{}" />
	<lightning:card iconName="standard:individual">
        <aura:set attribute="title">
            <div class="slds-text-heading_small"><b>General Information</b></div>
        </aura:set>
        <lightning:layout class="slds-form-element">
            <lightning:layoutItem class="slds-p-around_xx-small" size="4" alignmentBump="right">
                <div class="header-column">
                    <p class="field-title slds-form-element__label" title="Group Number">Group Number</p>
                    <p class="valueCls">
                        <lightning:formattedText value="{!v.generalInfo.GroupNumber}" class="slds-form-element__static"/>
                    </p>
                </div>
            </lightning:layoutItem>
            <lightning:layoutItem class="slds-p-around_xx-small" size="4" alignmentBump="left">
                <div class="header-column">
                    <p class="field-title slds-form-element__label" title="Patient Name">Patient Name</p>
                    <p class="valueCls">
                        <lightning:formattedText value="{!v.generalInfo.PatientName}" class="slds-form-element__static"/>
                    </p>
                </div>
            </lightning:layoutItem>
            <lightning:layoutItem class="slds-p-around_xx-small" size="4" alignmentBump="left">
                <div class="header-column">
                    <p class="field-title slds-form-element__label" title="Claim Split Indicator">Claim Split Indicator</p>
                    <p class="valueCls">
                        <lightning:formattedText value="{!v.generalInfo.ClaimSplitIndicator}" class="slds-form-element__static"/>
                    </p>
                </div>
            </lightning:layoutItem>
        </lightning:layout>
        <lightning:layout class="slds-form-element">
            <lightning:layoutItem class="slds-p-around_xx-small" size="4" alignmentBump="right">
                <div class="header-column">
                    <p class="field-title slds-form-element__label" title="Received Date">Received Date</p>
                    <p class="valueCls">
                        <lightning:formattedText value="{!v.generalInfo.ReceivedDate}" class="slds-form-element__static"/>
                    </p>
                </div>
            </lightning:layoutItem>
            <lightning:layoutItem class="slds-p-around_xx-small" size="4" alignmentBump="left">
                <div class="header-column">
                    <p class="field-title slds-form-element__label" title="Patient DOB">Patient DOB</p>
                    <p class="valueCls">
                        <lightning:formattedText value="{!v.generalInfo.PatientDOB}" class="slds-form-element__static"/>
                    </p>
                </div>
            </lightning:layoutItem>
            <lightning:layoutItem class="slds-p-around_xx-small" size="4" alignmentBump="left">
                <div class="header-column">
                    <p class="field-title slds-form-element__label" title="Split Claim Number">Split Claim Number</p>
                    <p class="valueCls">
                        <lightning:formattedText value="{!v.generalInfo.SplitClaimNumber}" class="slds-form-element__static"/>
                    </p>
                </div>
            </lightning:layoutItem>
        </lightning:layout>
        <lightning:layout class="slds-form-element">
            <lightning:layoutItem class="slds-p-around_xx-small" size="4" alignmentBump="right">
                <div class="header-column">
                    <p class="field-title slds-form-element__label" title="Claim Processed">Claim Processed</p>
                    <p class="valueCls">
                        <lightning:formattedText value="{!v.generalInfo.ClaimProcessed}" class="slds-form-element__static"/>
                    </p>
                </div>
            </lightning:layoutItem>
            <lightning:layoutItem class="slds-p-around_xx-small" size="4" alignmentBump="left">
                <div class="header-column">
                    <p class="field-title slds-form-element__label" title="Patient Account Number">Patient Account Number</p>
                    <p class="valueCls">
                        <lightning:formattedText value="{!v.generalInfo.PatientAccountNumber}" class="slds-form-element__static"/>
                    </p>
                </div>
            </lightning:layoutItem>
            <lightning:layoutItem class="slds-p-around_xx-small" size="4" alignmentBump="left">
                <div class="header-column">
                    <p class="field-title slds-form-element__label" title="Event Type">Event Type</p>
                    <p class="valueCls">
                        <lightning:formattedText value="{!v.generalInfo.ClaimEventType}" class="slds-form-element__static"/>
                    </p>
                </div>
            </lightning:layoutItem>
        </lightning:layout>
        <lightning:layout class="slds-form-element">
            <lightning:layoutItem class="slds-p-around_xx-small" size="4" alignmentBump="right">
                <div class="header-column">
                    <p class="field-title slds-form-element__label" title="Timely Filing Limit">Timely Filing Limit</p>
                    <p class="valueCls">
                        <lightning:formattedText value="{!v.generalInfo.TimelyFilingLimit}" class="slds-form-element__static"/>
                    </p>
                </div>
            </lightning:layoutItem>
            <lightning:layoutItem class="slds-p-around_xx-small" size="4" alignmentBump="left">
                <div class="header-column">
                    <p class="field-title slds-form-element__label" title="Bill Type">Bill Type</p>
                    <p class="valueCls">
                        <lightning:formattedText value="{!v.generalInfo.BillType}" class="slds-form-element__static"/>
                    </p>
                </div>
            </lightning:layoutItem>
            <lightning:layoutItem class="slds-p-around_xx-small" size="4" alignmentBump="left">
                <div class="header-column">
                    <p class="field-title slds-form-element__label" title="Event Sequence Number">Event Sequence Number</p>
                    <p class="valueCls">
                        <lightning:formattedText value="{!v.generalInfo.ClaimEventSequenceNumber}" class="slds-form-element__static"/>
                    </p>
                </div>
            </lightning:layoutItem>
        </lightning:layout>
        <lightning:layout class="slds-form-element">
            <lightning:layoutItem class="slds-p-around_xx-small" size="4" alignmentBump="right">
                <div class="header-column">
                    <p class="field-title slds-form-element__label" title="Submission Type">Submission Type</p>
                    <p class="valueCls">
                        <lightning:formattedText value="{!v.generalInfo.SubmissionType}" class="slds-form-element__static"/>
                    </p>
                </div>
            </lightning:layoutItem>
            <lightning:layoutItem class="slds-p-around_xx-small" size="4" alignmentBump="left">
                <div class="header-column">
                    <p class="field-title slds-form-element__label" title="Admit Date">Admit Date</p>
                    <p class="valueCls">
                        <lightning:formattedText value="{!v.generalInfo.AdmitDate}" class="slds-form-element__static"/>
                    </p>
                </div>
            </lightning:layoutItem>
            <lightning:layoutItem class="slds-p-around_xx-small" size="4" alignmentBump="left">
                <div class="header-column">
                    <p class="field-title slds-form-element__label" title="Related Claims">Related Claims</p>
                    <p class="valueCls">
                        <lightning:formattedText value="{!v.generalInfo.relatedIcns}" class="slds-form-element__static"/>
                    </p>
                </div>
            </lightning:layoutItem>
        </lightning:layout>
        <lightning:layout class="slds-form-element">
            <lightning:layoutItem class="slds-p-around_xx-small" size="4" alignmentBump="right">
                <div class="header-column">
                    <p class="field-title slds-form-element__label" title="DCN">DCN</p>
                    <p class="valueCls">
                        <lightning:formattedText value="{!v.generalInfo.DCN}" class="slds-form-element__static"/>
                    </p>
                </div>
            </lightning:layoutItem>
            <lightning:layoutItem class="slds-p-around_xx-small" size="4" alignmentBump="left">
                <div class="header-column">
                    <p class="field-title slds-form-element__label" title="Admit Time/Hr">Admit Time/Hr</p>
                    <p class="valueCls">
                        <lightning:formattedText value="{!v.generalInfo.AdmitTimeHR}" class="slds-form-element__static"/>
                    </p>
                </div>
            </lightning:layoutItem>
            <lightning:layoutItem class="slds-p-around_xx-small" size="4" alignmentBump="left">
                <div class="header-column">
                    
                </div>
            </lightning:layoutItem>
        </lightning:layout>
    </lightning:card>	
</aura:component>