<aura:component >
    <aura:attribute name="AutodocKey" type="string" access="public"/>
    <aura:attribute name="claimTotalsUHC" type="Map" default="{}" />
    <aura:attribute name="GUIkey" type="string" access="public" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    
    <div data-auto-doc="true" data-auto-doc-feature="{!v.AutodocKey}" data-auto-doc-section-key="Claim Totals - UHC" style="background:white !important;">
        <div class="autodocNotTableView" data-auto-doc-feature="{!v.AutodocKey}">
            <div class="slds-grid slds-wrap slds-form-element">
                <div class="slds-p-around_xx-small  slds-size_1-of-4 autodocField">
                    <div class="header-column">
                        <p class="field-title slds-form-element__label autodocFieldName" title="Status">Status</p>
                        <p class="valueCls autodocValue">
                            <div class="slds-form-element__static">{!v.claimTotalsUHC.Status}</div>
                        </p>
                    </div>
                </div>
                <div class="slds-p-around_xx-small  slds-size_1-of-4 autodocField">
                    <div class="header-column">
                        <p class="field-title slds-form-element__label autodocFieldName" title="Charged">Charged</p>
                        <p class="valueCls autodocValue">
                            <div class="slds-form-element__static">{!v.claimTotalsUHC.Charged}</div>
                        </p>
                    </div>
                </div>
                <div class="slds-p-around_xx-small  slds-size_1-of-4 autodocField">
                    <div class="header-column">
                        <p class="field-title slds-form-element__label autodocFieldName" title="Reduction">Reduction</p>
                        <p class="valueCls autodocValue">
                            <div class="slds-form-element__static">{!v.claimTotalsUHC.ReimbursementReduction}</div>
                        </p>
                    </div>
                </div>
                <div class="slds-p-around_xx-small  slds-size_1-of-4 autodocField">
                    <div class="header-column">
                        <p class="field-title slds-form-element__label autodocFieldName" title="Allowed">Allowed</p>
                        <p class="valueCls autodocValue">
                            <div class="slds-form-element__static">{!v.claimTotalsUHC.Allowed}</div>
                        </p>
                    </div>
                </div>
                <div class="slds-p-around_xx-small  slds-size_1-of-4 autodocField">
                    <div class="header-column">
                        <p class="field-title slds-form-element__label autodocFieldName" title="Penalty">Penalty</p>
                        <p class="valueCls autodocValue">
                            <div class="slds-form-element__static">{!v.claimTotalsUHC.Penalty}</div>
                        </p>
                    </div>
                </div>
                <div class="slds-p-around_xx-small  slds-size_1-of-4 autodocField">
                    <div class="header-column">
                        <p class="field-title slds-form-element__label autodocFieldName" title="Interest Paid">Interest Paid</p>
                        <p class="valueCls autodocValue">
                            <div class="slds-form-element__static">{!v.claimTotalsUHC.InterestPaid}</div>
                        </p>
                    </div>
                </div>
                <div class="slds-p-around_xx-small  slds-size_1-of-4 autodocField">
                    <div class="header-column">
                        <p class="field-title slds-form-element__label autodocFieldName" title="NY Surcharge Paid">NY Surcharge Paid</p>
                        <p class="valueCls autodocValue">
                            <div class="slds-form-element__static">{!v.claimTotalsUHC.NYSurchargePaid}</div>
                        </p>
                    </div>
                </div>
                <div class="slds-p-around_xx-small  slds-size_1-of-4 autodocField">
                    <div class="header-column">
                        <p class="field-title slds-form-element__label autodocFieldName" title="Health Plan Paid">Health Plan Paid</p>
                        <p class="valueCls autodocValue">
                            <div class="slds-form-element__static">{!v.claimTotalsUHC.HealthPlanPaid}</div>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
</aura:component>