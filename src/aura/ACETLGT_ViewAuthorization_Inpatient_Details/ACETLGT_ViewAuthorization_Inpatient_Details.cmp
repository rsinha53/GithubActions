<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,force:lightningQuickAction,lightning:isUrlAddressable"
                access="global" >
    
    <!-- List Values -->
    <aura:attribute name = "detailResult" type="Object" access="global"/>  <!--getAuthResult - Pass to child comp -->
    <aura:attribute name = "searchResult" type="Object" access="global"/>
    <aura:attribute name="authType" type="String" access="global" default="Inpatient"/>
    <aura:attribute name="AutodocKey" type="String" access="public"/>
    
    <lightning:card class="slds-p-around_small" >
        <aura:set attribute="title"><div class="slds-text-heading_small"><b>Inpatient Cases</b></div></aura:set>
        
        <div class="slds-grid slds-gutters_xxx-small"> 
            
            <div class="slds-col slds-size_1-of-1">
                <!-- Selected Row -->
                
                
                <div class="autodocTableView" data-auto-doc="true" data-auto-doc-feature="{!v.AutodocKey}" data-auto-doc-case-items="true" data-auto-doc-section-key="Inpatient Cases">   
                    <div class="slds-scrollable_x paddingTop">
                        <table class="slds-table preselect slds-table_bordered slds-table_cell-buffer slds-no-row-hover">
                        <thead>   
                            <tr>
                                <th>Authorization Number</th>
                               <!-- <th>Case Status</th> -->
                                <th>Pre Determination</th>
                                <th>Create Date</th>
                                <th>Expected Admit Date</th>
                                <th>Actual Admit Date</th>
                                <th>Expected Discharge Date</th>
                                <th>Actual Discharge Date</th>
                                <th>Facility/Status</th>
                                <th>Primary Diagnosis</th>
                                <th>POS</th>
                                <th>Service Desc/Detail</th>
                                <th>LOS</th>
                                
                            </tr>
                        </thead>
                        <tbody>
                            <aura:iteration items="{!v.searchResult}" var="sr">
                                
                                <tr class="slds-hint-parent">
                                    <td>{!sr.Authorization_Number}</td>
                                 <!--   <td>{!sr.Case_Status}</td>  -->
                                    <td>{!sr.Pre_Determination}</td>
                                    <td>{!sr.Create_Date}</td>
                                    <td>{!sr.Expected_Admit_Date}</td>
                                    <td>{!sr.Actual_Admit_Date}</td>
                                    <td>{!sr.Expected_Dscharg_Date}</td>
                                    <td>{!sr.Actual_Dscharg_Date}</td>
                                    <td>{!sr.Facility_Status}</td>
                                    <td>{!sr.Primary_Diagnosis}</td>
                                    <td>{!sr.POS}</td>
                                    <td>{!sr.ServiceDescDetail}</td>
                                    <td>{!sr.LOS}</td>
                                    
                                </tr>
                            </aura:iteration>
                        </tbody>    
                    </table>      
                    </div>
                </div>
            </div>
        </div>
    </lightning:card>
    
    <div class="slds-grid slds-gutters_xxx-small">
        
        
        <!-- Authorization Details , Diagnosis , Notification Type-->
        <div class="slds-col slds-size_2-of-8" >
            <div class="slds-box slds-p-around_none slds-theme_default slds-m-top_x-small">
                <lightning:accordion allowMultipleSectionsOpen="true" activeSectionName="AuthDetail" class="titleBold">
                    <lightning:accordionSection name="AuthDetail" label="Authorization Details">
                        <!-- <c:ACETLGT_AuthorizationDetail_DetailList/> -->
                        
                        
                        <div data-auto-doc="true" data-auto-doc-feature="{!v.AutodocKey}" data-auto-doc-section-key="Authorization Details"  data-auto-doc-section-column-indexes="1,2" >
                        <div class="autodocNotTableView" data-auto-doc-feature="{!v.AutodocKey}" data-auto-doc-case-items="false" >
                            <div class="slds-grid slds-form-element">
                                <div class="slds-p-around_xx-small slds-col_bump-right slds-size_6-of-12 autodocField" >
                                    <div class="header-column">
                                        <p class="field-title slds-form-element__label autodocFieldName" title="Authorization Number">Authorization Number</p>
                                        <p class="valueCls autodocValue">
                                            <div class="slds-form-element__static">{!v.detailResult.Authorization_Number}</div>
                                        </p>
                                    </div>
                                </div>
                                
                                <div class="slds-p-around_xx-small slds-col_bump-left slds-size_6-of-12 autodocField" >
                                    <div class="header-column">
                                        <p class="field-title slds-form-element__label autodocFieldName" title="Authorization Type">Authorization Type</p>
                                        <p class="valueCls autodocValue">
                                            <div class="slds-form-element__static">{!v.detailResult.Authorization_Type}</div>
                                        </p>
                                    </div>
                                </div>
                            </div>
                            
                            <div class="slds-grid slds-form-element">
                                <div class="slds-p-around_xx-small slds-col_bump-right slds-size_6-of-12 autodocField" >
                                    <div class="header-column">
                                        <p class="field-title slds-form-element__label autodocFieldName" title="Case Status">Case Status</p>
                                        <p class="valueCls autodocValue">
                                            <div class="slds-form-element__static">{!v.detailResult.Case_Status}</div>
                                        </p>
                                    </div>
                                </div>
                                
                                <div class="slds-p-around_xx-small slds-col_bump-left slds-size_6-of-12 autodocField" >
                                    <div class="header-column">
                                        <p class="field-title slds-form-element__label autodocFieldName" title="Case Status Reason">Case Status Reason</p>
                                        <p class="valueCls autodocValue">
                                            <div class="slds-form-element__static">{!v.detailResult.Case_Reason_Desc}</div>
                                        </p>
                                    </div>
                                </div>
                            </div>
                            
                            
                            <div class="slds-grid slds-form-element">
                                <div class="slds-p-around_xx-small slds-col_bump-right slds-size_6-of-12 autodocField" >
                                    <div class="header-column">
                                        <p class="field-title slds-form-element__label autodocFieldName" title="Expected Admission Date">Expected Admission Date</p>
                                        <p class="valueCls autodocValue">
                                            <div class="slds-form-element__static">{!v.detailResult.Expected_Admission_Date}</div>
                                        </p>
                                    </div>
                                </div>
                                
                                <div class="slds-p-around_xx-small slds-col_bump-left slds-size_6-of-12 autodocField" >
                                    <div class="header-column">
                                        <p class="field-title slds-form-element__label autodocFieldName" title="Expected Discharge Date">Expected Discharge Date</p>
                                        <p class="valueCls autodocValue">
                                            <div class="slds-form-element__static">{!v.detailResult.Expected_Discharge_Date}</div>
                                        </p>
                                    </div>
                                </div>
                            </div>
                            
                            
                            <div class="slds-grid slds-form-element">
                                <div class="slds-p-around_xx-small slds-col_bump-right slds-size_6-of-12 autodocField" >
                                    <div class="header-column">
                                        <p class="field-title slds-form-element__label fontType autodocFieldName" title="Expected Admission Date">Actual Admission Date</p>
                                        <p class="valueCls autodocValue">
                                            <div class="slds-form-element__static">{!v.detailResult.Actual_Admission_Date}</div>
                                        </p>
                                    </div>
                                </div>
                                
                                <div class="slds-p-around_xx-small slds-col_bump-left slds-size_6-of-12 autodocField" >
                                    <div class="header-column">
                                        <p class="field-title slds-form-element__label fontType autodocFieldName" title="Actual Discharge Date">Actual Discharge Date</p>
                                        <p class="valueCls autodocValue">
                                            <div class="slds-form-element__static">{!v.detailResult.Actual_Discharge_Date}</div>
                                        </p>
                                    </div>
                                </div>
                            </div>
                            
                            
                            <div class="slds-grid slds-form-element">
                                <div class="slds-p-around_xx-small slds-col_bump-right slds-size_6-of-12 autodocField" >
                                    <div class="header-column">
                                        <p class="field-title slds-form-element__label fontType autodocFieldName" title="PreDetermination">PreDetermination</p>
                                        <p class="valueCls autodocValue">
                                            <div class="slds-form-element__static">{!v.detailResult.Pre_Determination}</div>
                                        </p>
                                    </div>
                                </div>
                                
                                <div class="slds-p-around_xx-small slds-col_bump-left slds-size_6-of-12 autodocField" >
                                    <div class="header-column">
                                        <p class="field-title slds-form-element__label fontType autodocFieldName" title="Review Priority">Review Priority</p>
                                        <p class="valueCls autodocValue">
                                            <div class="slds-form-element__static">{!v.detailResult.Review_Priority}</div>
                                        </p>
                                    </div>
                                </div>
                            </div>
                            
                            
                            <div class="slds-grid slds-form-element">
                                <div class="slds-p-around_xx-small slds-col_bump-right slds-size_6-of-12 autodocField" >
                                    <div class="header-column">
                                        <p class="field-title slds-form-element__label fontType autodocFieldName" title="Manager/Queue">Manager/Queue</p>
                                        <p class="valueCls autodocValue">
                                            <div class="slds-form-element__static">{!v.detailResult.caseOwner}</div>
                                        </p>
                                    </div>
                                </div>
                                
                                <div class="slds-p-around_xx-small slds-col_bump-left slds-size_6-of-12 autodocField" >
                                    <div class="header-column">
                                        <p class="field-title slds-form-element__label autodocFieldName" title="Coverage Type">Coverage Type</p>
                                        <p class="valueCls autodocValue">
                                            <div class="slds-form-element__static">{!v.detailResult.Coverage_Type}</div>
                                        </p>
                                    </div>
                                </div>
                            </div>
                            
                            <div class="slds-grid slds-form-element">
                                <div class="slds-p-around_xx-small slds-col_bump-right slds-size_6-of-12 autodocField" >
                                    <div class="header-column">
                                        <p class="field-title slds-form-element__label fontType autodocFieldName" title="Discharge Location">Discharge Location</p>
                                        <p class="valueCls autodocValue">
                                            <div class="slds-form-element__static">{!v.detailResult.Discharge_Location}</div>
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div> 
                        </div>
                        
                        
                    </lightning:accordionSection>
                </lightning:accordion>
            </div>
            
            <div class="slds-box slds-p-around_none slds-theme_default slds-m-top_x-small">
                <lightning:accordion allowMultipleSectionsOpen="true" activeSectionName="Diagnosis" class="titleBold">
                    <lightning:accordionSection name="Diagnosis" label="Diagnosis"  >
                        <!--  <c:ACETLGT_Authorization_Diagnosis/> -->
                        <div class="autodocTableView" data-auto-doc="true" data-auto-doc-feature="{!v.AutodocKey}" data-auto-doc-case-items="false"  data-auto-doc-section-key="Diagnosis">   
                        
                        <div class="">
                            <table class="slds-table accordionTables slds-table_bordered slds-no-row-hover slds-table_fixed-layout " role="grid">
                                <thead>
                                    <tr>
                                        <th>Dx Code</th>
                                        <th>Description</th>
                                    </tr>
                                </thead>
                                <aura:iteration items="{!v.detailResult.diagnosisList}" var="diag">
                                    <tbody>    
                                        <tr class="slds-hint-parent">
                                            <td  class="slds-cell-wrap" >{!diag.Dx_Code}</td>
                                            <td  class="slds-cell-wrap" >{!diag.Dx_Description}</td>
                                        </tr>
                                    </tbody>    
                                </aura:iteration>
                            </table>      
                        </div>
                        </div>
                    </lightning:accordionSection>
                </lightning:accordion>
            </div>
            
            <div class="slds-box slds-p-around_none slds-theme_default slds-m-top_x-small">
                <lightning:accordion allowMultipleSectionsOpen="true" activeSectionName="NotType" class="titleBold">
                    <lightning:accordionSection name="NotType" label="Notification Type"  >
                        <!-- <c:ACETLGT_Authorization_NotificationType/> -->
                        <div class="autodocTableView" data-auto-doc="true" data-auto-doc-feature="{!v.AutodocKey}" data-auto-doc-case-items="false"  data-auto-doc-section-key="Notification Type">   
                        
                        <div class="">
                            <table class="slds-table accordionTables slds-table_bordered slds-no-row-hover slds-table_fixed-layout " role="grid">
                                <thead>
                                    <tr>
                                        <th>Notification Type</th>
                                        <th>Notified Date/Time</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <aura:iteration items="{!v.detailResult.notifications}" var="noti">
                                        <tr class="slds-hint-parent">
                                            <td class="slds-cell-wrap" >{!noti.Notification_Type}</td>
                                            <td class="slds-cell-wrap" >{!noti.Notified_Date_Time}</td>
                                        </tr>
                                    </aura:iteration>
                                </tbody>
                            </table>      
                        </div> 
                        
                        </div>
                    </lightning:accordionSection>
                </lightning:accordion>
            </div> 
            
            
        </div>
        
        
        <!-- Tables for Inpatient -->
        <div class="slds-col slds-size_6-of-8" >
            <div class="slds-box slds-p-around_none slds-theme_default slds-m-top_x-small">
                <lightning:accordion allowMultipleSectionsOpen="true" activeSectionName="Sline" class="titleBold">
                    <lightning:accordionSection name="Sline" label="Service Line"  >
                        
                        
                            <div class="autodocTableView" data-auto-doc="true" data-auto-doc-feature="{!v.AutodocKey}" data-auto-doc-case-items="false"  data-auto-doc-section-key="Service Line">   
                            <div class="slds-scrollable_x">
                            <table class="slds-table accordionTables slds-table_bordered slds-table_cell-buffer slds-no-row-hover">
                                <thead>
                                    <tr>
                                        <th>Authorization Number</th>
                                        <th>Procedure Code</th>
                                        <th>Description</th>
                                        <th>Proc Text</th>
                                        <th>Expected Procedure Date</th>
                                        <th>Acutal Procedure Date</th>
                                        <th>Provider/Status</th>
                                        <th>Decision Outcome</th>
                                        <th>Decision Subtype</th>
                                        <th>Decision Rendered Date</th>
                                        <th>Claim Remark Code</th>
                                        <th>Gap Outcome</th>
                                        <th>Decision Reason</th>
                                        <th>Decision Comment</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <aura:iteration items="{!v.detailResult.authServiceDetails}" var="sl">
                                        <tr class="slds-hint-parent">
                                            <td>{! sl.Service_Authorization_Number}</td>
                                            <td>{! sl.Procedure_Code}</td>
                                            <td>{! sl.Procedure_Description}</td>
                                            <td>{! sl.Proc_Text}</td>
                                            <td>{! sl.Expected_Procedure_Date}</td>
                                            <td>{! sl.Actual_Procedure_Date}</td>
                                            <td>{! sl.Service_Provider_Status}</td>
                                            <td>{! sl.Service_Decision_Outcome}</td>
                                            <td>{! sl.Service_Decision_Sub_Type}</td>
                                            <td>{! sl.Service_Decision_Rendered_Date}</td>
                                            <td>{! sl.Service_Claim_Remark_Code}</td>
                                            <td>{! sl.Service_Gap_Outcome}</td>
                                            <td>{! sl.Service_Decision_Reason}</td>
                                            <td>{! sl.Service_Decision_Comment}</td>
                                        </tr>
                                    </aura:iteration>
                                </tbody>    
                            </table>      
                        	</div>
                            </div>
                        
                        
                        
                        
                    </lightning:accordionSection>
                </lightning:accordion>
            </div>
            
            
            
            
            <div class="slds-box slds-p-around_none slds-theme_default slds-m-top_x-small">
                <lightning:accordion allowMultipleSectionsOpen="true" activeSectionName="Fline" class="titleBold">
                    <lightning:accordionSection name="Fline" label="Facility Line" >
                        
                        
                            <div class="autodocTableView" data-auto-doc="true" data-auto-doc-feature="{!v.AutodocKey}" data-auto-doc-case-items="false"  data-auto-doc-section-key="Facility Line">   
                        	<div class="slds-scrollable_x">
                            <table class="slds-table accordionTables slds-table_bordered slds-table_cell-buffer slds-no-row-hover">
                                <thead>
                                    <tr>
                                        <th>Authorization Number</th>
                                        <th>POS</th>
                                        <th>Srvc Detail</th>
                                        <th>Srvc Desc</th>
                                        <th>Provider/Status</th>
                                        <th>Decision Outcome</th>
                                        <th>Decision Sub Type</th>
                                        <th>Decision Rendered Date</th>
                                        <th>Claim Remark Code</th>
                                        <th>Gap Outcome</th>
                                        <th>Decision Reason</th>
                                        <th>Decision Comment</th>
                                        
                                    </tr>
                                </thead>
                                <tbody>
                                    <aura:iteration items="{!v.detailResult.authFacilityDetails}" var="fl"> 
                                        <tr class="slds-hint-parent">
                                            <td>{! fl.Facility_Authorization_Number}</td>
                                            <td>{! fl.POS}</td>
                                            <td>{! fl.Service_Detail}</td>
                                            <td>{! fl.Service_Desc}</td>
                                            <td>{! fl.Facility_Provider_Status}</td>
                                            <td>{! fl.Facility_Decision_Outcome}</td>
                                            <td>{! fl.Facility_Decision_Sub_Type}</td>
                                            <td>{! fl.Facility_Decision_Rendered_Date}</td>
                                            <td>{! fl.Facility_Claim_Remark_Code}</td>
                                            <td>{! fl.Facility_Gap_Outcome}</td>
                                            <td>{! fl.Facility_Decision_Reason}</td>
                                            <td>{! fl.Facility_Decision_Comment}</td>
                                            
                                        </tr>
                                    </aura:iteration>
                                </tbody>    
                            </table>      
                        	</div>
                            </div>
                        
                        
                    </lightning:accordionSection>
                </lightning:accordion>
            </div>           
            
            
            <div class="slds-box slds-p-around_none slds-theme_default slds-m-top_x-small">
                <lightning:accordion allowMultipleSectionsOpen="true" activeSectionName="Beddd" class="titleBold">
                    <lightning:accordionSection name="Beddd" label="Bed Day Decision" >
                        
                        
                            <div class="autodocTableView" data-auto-doc="true" data-auto-doc-feature="{!v.AutodocKey}" data-auto-doc-case-items="false"  data-auto-doc-section-key="Bed Day Decision">   
                        	<div class="slds-scrollable_x">
                            <table class="slds-table accordionTables slds-table_bordered slds-table_cell-buffer slds-no-row-hover">
                                <thead>
                                    <tr>
                                        <th>Begin Date</th>
                                        <th>End Date</th>
                                        <th>Decision Update</th>
                                        <th>Decision</th>
                                        <th>Reason</th>
                                        <th>Bed type</th>
                                        <th>Claim Remark Code</th>
                                        
                                    </tr>
                                </thead>
                                <tbody>
                                    <aura:iteration items="{!v.detailResult.bedDayDecision}" var="bdd">
                                        <tr class="slds-hint-parent">
                                            <td>{! bdd.bedDatesStart}</td>
                                            <td>{! bdd.bedDatesStop}</td>
                                            <td>{! bdd.decisionUpdateDateTime}</td>
                                            <td>{! bdd.decisionOutcomeDescription}</td>
                                            <td>{! bdd.decisionReasonDescription}</td>
                                            <td>{! bdd.bedTypeDescription}</td>
                                            <td>{! bdd.systemClaimRemarkCode}</td>
                                        </tr>
                                    </aura:iteration>
                                </tbody>    
                            </table>      
                        	</div>
                            </div>
                        
                        
                        
                    </lightning:accordionSection>
                </lightning:accordion>
            </div>
            
            
            <div class="slds-box slds-p-around_none slds-theme_default slds-m-top_x-small">
                <lightning:accordion allowMultipleSectionsOpen="true" activeSectionName="Prov" class="titleBold">
                    <lightning:accordionSection name="Prov" label="Providers"  >
                        
                        
                            <div class="autodocTableView" data-auto-doc="true" data-auto-doc-feature="{!v.AutodocKey}" data-auto-doc-case-items="false"  data-auto-doc-section-key="Providers">   
                        	<div class="slds-scrollable_x">
                            <table class="slds-table accordionTables slds-table_bordered slds-table_cell-buffer slds-no-row-hover" role="grid">
                                <thead>    
                                    <tr>
                                        <th>Provider Role</th>
                                        <th>Provider ID</th>
                                        <th>Srvc Provider/Status</th>
                                        <th>TIN</th>
                                        <th>Address 1</th>
                                        <th>City</th>
                                        <th>State</th>
                                        <th>Zip</th>
                                        <th>Speciality</th>
                                    </tr>
                                </thead>
                                
                                <tbody>
                                    <aura:iteration items="{!v.detailResult.authProviderDetails}" var="prov">
                                        <tr class="slds-hint-parent">
                                            <td>{! prov.Provider_Facility_Role}</td>
                                            <td>{! prov.Provider_ID}</td>
                                            <td>{! prov.Facility_Service_Provider_Status}</td>
                                            <td>{! prov.TIN}</td>
                                            <td>{! prov.Address_1}</td>
                                            <td>{! prov.City}</td>
                                            <td>{! prov.State}</td>
                                            <td>{! prov.ZIP}</td>
                                            <td>{! prov.Speciality}</td>
                                        </tr>
                                    </aura:iteration>
                                </tbody>    
                            </table>      
                        	</div>
                            </div>
                        
                        
                        
                        
                    </lightning:accordionSection>
                </lightning:accordion>
            </div>
            
            
            <div class="slds-box slds-p-around_none slds-theme_default slds-m-top_x-small">
                <lightning:accordion allowMultipleSectionsOpen="true" activeSectionName="" class="titleBold">
                    <lightning:accordionSection name="A" label="Notes"  >
                        
                       
                            <div class="autodocTableView" data-auto-doc="true" data-auto-doc-feature="{!v.AutodocKey}" data-auto-doc-case-items="false"  data-auto-doc-section-key="Notes">   
                             <div class="slds-scrollable_x">
                            <table class="slds-table accordionTables slds-table_bordered slds-table_resizable-cols slds-table_cell-buffer slds-no-row-hover">
                                <thead>
                                    <tr>
                                        <th>Create Date</th>
                                        <th>Created By</th>
                                        <th>Department</th>
                                        <th>Note Type</th>
                                        <th>Subject Type</th>
                                        <th>Note Details</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <aura:iteration items="{!v.detailResult.notesList}" var="notes">
                                        <tr class="slds-hint-parent">
                                            <td>{!notes.Create_Date}</td>
                                            <td>{!notes.Created_By}</td>
                                            <td>{!notes.Department}</td>
                                            <td>{!notes.Note_Type}</td>
                                            <td>{!notes.Subject_Type}</td>
                                            <td>{!notes.Note_details}</td>
                                        </tr>
                                    </aura:iteration>
                                </tbody>
                            </table>      
                        </div>  
                            </div>
                        
                    </lightning:accordionSection>
                </lightning:accordion>
            </div>
            
            
            <div class="slds-box slds-p-around_none slds-theme_default slds-m-top_x-small">
