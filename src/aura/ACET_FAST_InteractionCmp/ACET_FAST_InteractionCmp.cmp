<aura:component controller="ACET_FAST_InteractionCtrl" implements="force:appHostable,lightning:isUrlAddressable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction"  >
    <!-- Attributes -->
    <aura:attribute name="intWrap" type="ACET_FAST_InteractionCtrl.interactionWrapper"/>
    <aura:attribute name="availableMarketList" type="List" default="[]" description="Market List"/>
    <aura:attribute name="selectedMarketList" type="List" default="[]" description="Selected Markets"/>
    <aura:attribute name="availableProductList" type="List" default="[]" description="Product List"/>
    <aura:attribute name="selectedProductList" type="List" default="[]" description="Selected Products"/>
    <aura:attribute name="accountRecId" type="string"  default="" description="person account reord`s id"/>
    <aura:attribute name="caseRecType" type="String" default="Reactive Resolution"/>
    <!-- Attributes -->
    
    <!-- WorkSpace -->
    <lightning:workspaceAPI aura:id="workspace" />
    <!-- Workspace -->
    
    <!-- Handlers -->
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <!-- Handlers -->
    
    <!-- Spinner -->
    <aura:if aura:id="memberCardSpinnerAI" isTrue="false">
        <div class="slds-p-around_xx-large">
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" style="margin:5%" class="slds-modal slds-fade-in-open">
                <div class="demo-only" style="height:6rem">
                    <div class="slds-spinner_container">
                        <div role="status" class="slds-spinner slds-spinner_medium slds-spinner_brand">
                            <span class="slds-assistive-text">Loading</span>
                            <div class="slds-spinner__dot-a"></div>
                            <div class="slds-spinner__dot-b"></div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
        <div class="slds-backdrop slds-backdrop_open"></div>
    </aura:if>
    <!-- Spinner -->
    
    <!--Mark Up -->
    <div>
        
        <!-- Page Header -->
        <lightning:card>
            <aura:set attribute="actions">
                <lightning:buttonGroup>
                    <c:ACET_HippaGuidelines showButton="true"/>
                    <lightning:button label="Misdirect" />
                    <c:SAE_Alerts aura:id="alertsAI" 
                                  providerId="{!v.intWrap.caseRec.PC_Provider_TIN__c}"
                                  />
                </lightning:buttonGroup>
            </aura:set>
            <aura:set attribute="title">
                <div class="slds-page-header" style="background:#fff">
                    <div class="slds-page-header__row">
                        <div class="slds-page-header__col-title">
                            <div class="slds-media">
                                <div class="slds-media__figure" >
                                    <span class="slds-icon_container slds-icon-standard-opportunity" title="Interaction">
                                        <lightning:icon iconName="standard:contact_list" alternativeText="Interaction" title="Interaction" />
                                        <span class="slds-assistive-text" >Interaction</span>
                                    </span>
                                </div>
                                <div class="slds-media__body">
                                    <div class="slds-page-header__name">
                                        <div class="slds-page-header__name-title">
                                            <h1>
                                                <span class="slds-page-header__title slds-truncate" title="Interaction Overview" style="font-weight:bold;">Interaction Overview</span>
                                            </h1>
                                        </div>
                                    </div>
                                    <p class="slds-page-header__name-meta"><a href="{!'/'+v.intWrap.personAccount.Id}">{!v.intWrap.personAccount.Name}</a></p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </aura:set>
        </lightning:card>
        <!-- Page Header -->
        
        <!-- Interaction Overview -->
        <lightning:card >
            <aura:set attribute="title">
                <div style="font-weight:bold;">
                    <p>Provider: {!v.intWrap.personAccount.Name}</p>
                </div>
            </aura:set>
            <div class="slds-p-horizontal_large">
                <div class="slds-grid slds-gutters" >
                    <div class="slds-col">
                        <div>
                            <label>Tax Id</label>
                        </div>
                        <div>
                            <h3>{!v.intWrap.personAccount.Tax_Id__c}</h3>
                        </div>
                    </div>
                    <div class="slds-col">
                        <div>
                            <label>Phone</label>
                        </div>
                        <div>
                            <h3>{!v.intWrap.personAccount.Provider_Contact_Phone__c}</h3>
                        </div>
                    </div>
                    <div class="slds-col">
                        <div>
                            <label>Contact Name</label>
                        </div>
                        <div>
                            <h3>{!v.intWrap.personAccount.Provider_Contact__c}</h3>
                        </div>
                    </div>
                    <div class="slds-col">
                        <div>
                            <label>Provider Contact Preference</label>
                        </div>
                        <div>
                            <h3>{!v.intWrap.personAccount.Provider_Contact_Preference__c}</h3>
                        </div>
                    </div>
                    <div class="slds-col">
                        <div>
                            <label>Provider Email</label>
                        </div>
                        <div>
                            <h3>{!v.intWrap.personAccount.Provider_Contact_Email__c}</h3>
                        </div>
                    </div>
                    <div class="slds-col">
                        <div>
                            <label>Par Provider</label>
                        </div>
                        <div>
                            <h3>{!v.intWrap.personAccount.Par_Provider__c}</h3>
                        </div>
                    </div>
                </div>
            </div>
        </lightning:card>
        <!-- Interaction Overview -->
        
        <!-- Case Record Type -->
        <div style="margin-top:1%">
            <lightning:card>
                <aura:set attribute="title">
                    <div style="font-weight:bold;"><p>Case Record Type</p></div>
                </aura:set>
                <div class="slds-p-horizontal_large">
                    <label>Case Record Type</label>
                    <lightning:input aura:id="caseRecordTypeId" 
                                     name="Case Record Type" 
                                     label="Case Record Type" 
                                     disabled="true" 
                                     value="{!v.caseRecType}"
                                     variant="label-hidden"/>
                </div>
            </lightning:card>
        </div>
        <!-- Case Record Type -->
        
        <!-- Fast Case -->
        <aura:if isTrue="{!v.caseRecType=='Reactive Resolution'}">
            <lightning:recordEditForm aura:id="fastCaseForm" 
                                      objectApiName="Case">
                <div style="margin-top:1%">
                    <lightning:card >
                        <aura:set attribute="title">
                            <div style="font-weight:bold;"><p>Issue Detail</p></div>
                        </aura:set>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_6-of-12">
                                <div class="slds-p-around_medium">
                                    <label><span style="color:red">*</span>Subject</label>
                                    <lightning:textarea aura:id="subjectId" 
                                                        class="heightText" 
                                                        name="Subject" 
                                                        label="Subject" 
                                                        value="{!v.intWrap.caseRec.Subject}"
                                                        required="true"
                                                        messageWhenBadInput="Complete this field"
                                                        variant="label-hidden"/> 
                                </div>
                            </div>
                            <div class="slds-col slds-size_6-of-12">
                                <div class="slds-p-around_medium">
                                    <label><span style="color:red">*</span>Description</label>
                                    <lightning:textarea aura:id="descId" 
                                                        name="Description" 
                                                        class="heightText"  
                                                        label="Description" 
                                                        value="{!v.intWrap.caseRec.Description}"
                                                        required="true" 
                                                        messageWhenBadInput="Complete this field"
                                                        variant="label-hidden"/> 
                                </div>
                            </div>
                        </div>
                    </lightning:card>
                </div>
                <!-- Issue Detail -->
                
                <!-- Details -->
                <div style="margin-top:1%">
                    <lightning:card>
                        <aura:set attribute="title">
                            <div style="font-weight:bold;"><p>Issue Detail</p></div>
                        </aura:set>
                        <div class="slds-grid slds-wrap" style="padding-right: 1%;">
                            <div class="slds-col slds-size_4-of-12">
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium taxIdLabelCls">
                                    <label for="taxId">Tax ID</label>
                                    <lightning:inputfield aura:id="taxId" 
                                                          fieldName="PC_Provider_TIN__c" 
                                                          variant="label-hidden"
                                                          value="{!v.intWrap.caseRec.PC_Provider_TIN__c}"
                                                          disabled="true"/>
                                </div>
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium accountCls">
                                    <label for="accountID"><span style="color:red">*</span>Account</label>
                                    <lightning:inputField aura:id="accountRecId" 
                                                          fieldName="AccountId"  
                                                          value="{!v.intWrap.caseRec.AccountId}"
                                                          variant="label-hidden"
                                                          required="true"
                                                          disabled="true"
                                                          messageWhenBadInput="Complete this field"/>
                                </div>
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <lightning:select aura:id="caseOriginId" 
                                                      name="Case Origin" 
                                                      disabled="{!empty(v.intWrap.caseOriginList)}" 
                                                      label="Case Origin" 
                                                      required="true" 
                                                      value="{!v.intWrap.caseRec.Origin}">
                                        <option value="">--None--</option>
                                        <aura:iteration items="{!v.intWrap.caseOriginList}" var="caseOrigin">
                                            <option value="{!caseOrigin}" >{!caseOrigin}</option>
                                        </aura:iteration>
                                    </lightning:select>
                                </div>
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <label for="providerTypeID"><span style="color:red">* </span>Provider Type</label>
                                    <lightning:inputField aura:id="providerTypeID" 
                                                          fieldName="Provider_Type__c"  
                                                          value="{!v.intWrap.caseRec.Provider_Type__c}"
                                                          variant="label-hidden"
                                                          required="true"
                                                          messageWhenBadInput="Complete this field"/>
                                </div>
              			        <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <label for="speacialityId">Specialty Team</label>
                                    <lightning:inputfield aura:id="speacialityId" 
                                                          fieldName="Specialty_Team__c" 
                                                          variant="label-hidden"
                                                          value="{!v.intWrap.caseRec.Specialty_Team__c}"/>
                                </div>

                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <lightning:dualListbox aura:id="fastselectedmarketsId"
                                                           name="Markets"
                                                           label="Markets"
                                                           options="{!v.availableMarketList}"
                                                           value="{!v.selectedMarketList}"
                                                           onchange="{!c.handleMarketChange}"
                                                           required="true"
                                                           messageWhenBadInput="Complete this field"
                                                           size="8"/>
                                </div>                           

                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <lightning:select aura:id="topicId"
                                                    label="Topic" 
                                                    required="true"  
                                                    name="Topic" 
                                                    value="{!v.intWrap.caseRec.Topic__c}"
                                                    onchange="{!c.handleTopicChange}">
                                        <option value="">--None--</option>
                                        <aura:iteration items="{!v.intWrap.topicList}" var="topic">
                                            <option value="{!topic}" >{!topic}</option>
                                        </aura:iteration>
                                    </lightning:select>
                                </div>
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <lightning:select aura:id="typeId" 
                                                    disabled="true" 
                                                    label="Type"                   
                                                    name="Type" 
                                                    value="{!v.intWrap.caseRec.Type__c}"
                                                   >
                                        <option value="">--None--</option>
                                        <aura:iteration items="{!v.intWrap.typeList}" var="type">
                                            <option value="{!type}" >{!type}</option>
                                        </aura:iteration>
                                    </lightning:select>
                                </div>
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <lightning:select aura:id="subTypeId"
                                                    label="Sub-Type" 
                                                    disabled="true" 
                                                    name="Sub-Type" 
                                                    value="{!v.intWrap.caseRec.Subtype__c}">
                                        <option value="">--None--</option>
                                        <aura:iteration items="{!v.intWrap.subTypeList}" var="subType">
                                            <option value="{!subType}" >{!subType}</option>
                                        </aura:iteration>
                                    </lightning:select>
                                </div>
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <label for="providerRegionID">Provider Region</label>
                                    <lightning:inputField  fieldName="PC_Provider_Region__c"  
                                                        aura:id="providerRegionID"  
                                                        value="{!v.intWrap.caseRec.PC_Provider_Region__c}"
                                                        variant="label-hidden"/>
                                </div>
                                                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    
                                    <label for="Origin">UHG Restriction</label>
                                    <lightning:inputfield aura:id="uhgRectId" 
                                                          fieldName="UHG_Restriction__c" 
                                                          variant="label-hidden"
                                                          value="{!v.intWrap.caseRec.UHG_Restriction__c}"/>
                                </div>
                                <!--- added by Santhosh Start-->
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <ui:inputCheckbox aura:id="complaint" label="Complaint" value="{!v.intWrap.caseRec.Complaint__c}" />
                                </div>
                                <!--- added by Santhosh End-->
                            </div>
                        </div>
                    </lightning:card>
                </div>
                <!-- Details -->
                
                <!-- Submitter Details -->
                <div style="margin-top:1%">
                    <lightning:card>
                        <aura:set attribute="title">
                            <div style="font-weight:bold;"><p>Submitter Details</p></div>
                        </aura:set>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_6-of-12">
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <label for="sContactId"><span style="color:red">* </span>Submitter Contact</label>
                                    <lightning:inputfield aura:id="sContactId" 
                                                          fieldName="PC_Internal_Contact_Name__c" 
                                                          variant="label-hidden"
                                                          value="{!v.intWrap.caseRec.PC_Internal_Contact_Name__c}"
                                                          required="true"
                                                          messageWhenBadInput="Complete this field"/>
                                </div>
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <label for="SubmitterPhoneId"><span style="color:red">* </span>Submitter Contact Phone</label>
                                    <lightning:input type="text" 
                                                     minlength="10" 
                                                     maxlength="10"
                                                     onblur="{!c.formatPhoneNumber}" 
                                                     aura:id="SubmitterPhoneId" 
                                                     label="Submitter Contact Phone" 
                                                     name="Submitter Contact Phone"
                                                     value="{!v.intWrap.caseRec.PC_Internal_Contact_Phone__c}"
                                                     pattern="[0-9]{10}"  
                                                     messageWhenPatternMismatch ="Enter 10 digits"
                                                     required="true"
                                                     variant="label-hidden"/>
                                </div>
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <label for="contactEmailId"><span style="color:red">* </span>Submitter Contact E-mail</label>
                                    <lightning:input type="email" 
                                                     aura:id="contactEmailId" 
                                                     name="Submitter Contact E-mail" 
                                                     required="true" 
                                                     value="{!v.intWrap.caseRec.PC_Internal_Contact_Email__c}" 
                                                     label="Submitter Email"
                                                     variant="label-hidden"/>
                                </div>
                            </div>
                            <div class="slds-col slds-size_6-of-12">
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <label for="submittingDeptId"><span style="color:red">* </span>Submitting Department</label>
                                    <lightning:inputfield aura:id="submittingDeptId" 
                                                          fieldName="Submitting_Department__c" 
                                                          variant="label-hidden"
                                                          value="{!v.intWrap.caseRec.Submitting_Department__c}"
                                                          required="true"
                                                          messageWhenBadInput="Complete this field"/>
                                </div>
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium submitterLabelCls">
                                    <label for="submittingDateId"><span style="color:red">* </span>Submitter Recieved Date</label>
                                    <lightning:inputfield aura:id="submittingDateId" 
                                                          fieldName="Original_Received_Date__c" 
                                                          variant="label-hidden"
                                                          value="{!v.intWrap.caseRec.Original_Received_Date__c}"
                                                          required="true"
                                                          messageWhenBadInput="Complete this field" onchange="{!c.onchangeSubmitDate}" />
                                    
                                </div>
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <label>Reason for Submission Delay</label>
                                    <lightning:select aura:id="rsdId" 
                                                      name="Reason for Submission Delay" 
                                                      disabled="{!empty(v.intWrap.sumissionDelayReasonList)}" 
                                                      label="Reason for Submission Delay" 
                                                      value="{!v.intWrap.pirDetailRecord.Reason_for_Submission_Delay__c}"
                                                      variant="label-hidden">
                                        <option value="">--None--</option>
                                        <aura:iteration items="{!v.intWrap.sumissionDelayReasonList}" var="subDelay">
                                            <option value="{!subDelay}" >{!subDelay}</option>
                                        </aura:iteration>
                                    </lightning:select>
                                </div>
                            </div>
                        </div>
                    </lightning:card>
                </div>
                <!-- Submitter Details -->
                
                <!-- Provider Contact Info -->
                <div style="margin-top:1%">
                    <lightning:card>
                        <aura:set attribute="title">
                            <div style="font-weight:bold;"><p>Provider Contact Info</p></div>
                        </aura:set>
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_6-of-12">
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <label for="econtactId"><span style="color:red">* </span>External Contact Name</label>
                                    <lightning:inputfield aura:id="econtactId" 
                                                          fieldName="PC_External_Contact_Name__c" 
                                                          variant="label-hidden"
                                                          value="{!v.intWrap.caseRec.PC_External_Contact_Name__c}"
                                                          required="true"
                                                          messageWhenBadInput="Complete this field"/>
                                </div>  
                            </div>
                            <div class="slds-col slds-size_6-of-12">
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <label for="pcontactprefId"><span style="color:red">* </span>Provider Contact Preference <lightning:helptext content="Type of contact provider prefers at closure."/></label>
                                    <lightning:inputfield aura:id="pcontactprefId" 
                                                          fieldName="Provider_Contact_Preference__c" 
                                                          variant="label-hidden"
                                                          value="{!v.intWrap.caseRec.Provider_Contact_Preference__c}"
                                                          required="true"
                                                          class="hideIcon"
                                                          messageWhenBadInput="Complete this field"/>
                                </div>
                            </div>
                            <div class="slds-col slds-size_6-of-12">
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <label for="econtactPhoneId"><span style="color:red">* </span>External Contact Phone</label>
                                    <lightning:inputfield aura:id="econtactPhoneId" 
                                                          fieldName="PC_External_Contact_Phone__c" 
                                                          variant="label-hidden"
                                                          value="{!v.intWrap.caseRec.PC_External_Contact_Phone__c}"
                                                          required="true"
                                                          messageWhenBadInput="Complete this field"/>
                                </div>
                            </div>                             
                            <div class="slds-col slds-size_6-of-12">
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <label for="parproviderId"><span style="color:red">* </span>Par Provider</label>
                                    <lightning:inputfield aura:id="parproviderId" 
                                                          fieldName="PC_Is_There_Active_PRs__c" 
                                                          variant="label-hidden"
                                                          value="{!v.intWrap.caseRec.PC_Is_There_Active_PRs__c}"
                                                          required="true"
                                                          messageWhenBadInput="Complete this field"/>
                                </div>
                            </div>
                            <div class="slds-col slds-size_6-of-12">
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                    <label for="econtactemailId"><span style="color:red">* </span>External Contact Email</label>
                                    <lightning:inputfield aura:id="econtactemailId" 
                                                          fieldName="PC_External_Contact_Email__c" 
                                                          variant="label-hidden"
                                                          value="{!v.intWrap.caseRec.PC_External_Contact_Email__c}"
                                                          required="true"
                                                          messageWhenBadInput="Complete this field"/>
                                </div>
                            </div>
                            <div class="slds-col slds-size_6-of-12">
                                <div class="slds-p-horizontal_medium slds-p-bottom_medium">                               
                                    <label for="scpcId"><span style="color:red">* </span>Submitter to Contact Provider at Closure</label>
                                    <lightning:select aura:id="scpcId" 
                                                      name="Submitter to Contact Provider at Closure" 
                                                      label="Submitter to Contact Provider at Closure" 
                                                      value="{!v.intWrap.pirDetailRecord.Submitter_to_Contact_Provider_at_Closure__c}"                                                       
                                                      variant="label-hidden"
                                                      >
                                        <option value="">--None--</option>
                                        <option value="Yes">Yes</option>
                                        <option value="No">No</option>
                                    </lightning:select>
                                </div>
                            </div>
                        </div>
                    </lightning:card>
                </div>
                <!-- Provider Contact Info -->
                
            </lightning:recordEditForm>
        </aura:if>
        <!-- Fast Case -->
        
        <!-- PIP Case -->
        <aura:if isTrue="{!v.caseRecType=='Proactive Action'}">
            <div style="margin-top:1%">
                <lightning:recordEditForm aura:id="PIPCaseForm" objectApiName="Case">
                    <!-- Issue Details -->
                    <lightning:card >
                        <aura:set attribute="title">
                            <div style="font-weight:bold;"><p>Issue Details</p></div>
                        </aura:set>
                        <div class="slds-p-horizontal_large">
                            <div class="slds-grid slds-gutters">
                                <div class="slds-col">
                                    <label><span style="color:red">*</span>Opportunity/Project Name</label>
                                    <lightning:textarea aura:id="OSubjectId" 
                                                        class="heightText" 
                                                        name="Opportunity/Project Name" 
                                                        required="true" 
                                                        label="Opportunity/Project Name" 
                                                        value="{!v.intWrap.caseRec.Subject}"
                                                        variant="label-hidden"/> 
                                </div>
                                <div class="slds-col">
                                    <label><span style="color:red">*</span>Opportunity Description/Problem Statement</label>
                                    <lightning:textarea aura:id="ODescId" 
                                                        name="Opportunity Description/Problem Statement" 
                                                        class="heightText"  
                                                        required="true" 
                                                        label="Opportunity Description/Problem Statement" 
                                                        value="{!v.intWrap.caseRec.Description}"
                                                        variant="label-hidden"/> 
                                </div>
                            </div>
                        </div>
                    </lightning:card>
                    <!-- Issue Details -->
                    
                    <!-- Details -->
                    <lightning:card >
                        <aura:set attribute="title">
                            <div style="font-weight:bold;"><p>Details</p></div>
                        </aura:set>
                        <div class="slds-p-horizontal_large" style="margin-bottom:1%;">
                            <div class="slds-grid slds-gutters" >
                                <div class="slds-col slds-size_4-of-12">
                                    <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                        <label for="Origin"><span style="color:red">*</span>Case Origin</label>
                                        <lightning:inputfield aura:id="originId" 
                                                              fieldName="Origin" 
                                                              variant="label-hidden"
                                                              value="{!v.intWrap.caseRec.Origin}"/>
                                    </div>
                                    <div class="slds-p-horizontal_medium slds-p-bottom_medium">
                                        <lightning:select aura:id="lobId" 
                                                          value="{!v.intWrap.caseRec.PC_Line_of_Business_Platform__c}" 
                                                          name="LOB" 
                                                          label="Line Of Business" 
