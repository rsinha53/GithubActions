                                </th>
                                <th class="table_column_header" style="cursor:pointer; width:9%;" scope="col">NETWORK ID</th>
                                <th class="table_column_header" style="cursor:pointer; width:7%;" scope="col">DEC #</th>
                                <th class="table_column_header" style="cursor:pointer; width:6%;" scope="col">PMG</th>
                                <th class="table_column_header" style="cursor:pointer; width:7%;" scope="col">RISK</th>
                                <th class="table_column_header" style="cursor:pointer; width:10%;" scope="col">PMG CRITERIA</th>
                                <th class="table_column_header" style="cursor:pointer; width:12%;" scope="col">EFFECTIVE-CANCEL</th>
                                <th class="table_column_header" style="cursor:pointer; width:8%;" scope="col">TYPE</th>
                                <th class="table_column_header" style="cursor:pointer; width:10%;" scope="col">
                                    <c:ACET_HoverPopup aura:id="PHSpopup" moveRight="-350" description="See below for description:" descriptionList="[ 'N - No new patients accepted', 'Y - New patients are being accepted', 'E - New patients accepted on exception basis only', 'R - Accepting new patients with restrictions (age, gender, etc).' ]" isNubbinLeft="false"/>
                                    <p data-popupId="PHSpopup" onmouseover="{!c.togglePopup}" onmouseout="{!c.togglePopup}"><span class="dashedLine">NEW PATIENTS?</span></p>
                                </th>
                                <th class="autodoc-case-item-resolved table_column_header">RESOLVED</th>
                            </tr>
                        </thead>
                        <tbody>
                            <aura:if isTrue="{!v.phsTableIsEmpty}">
                                <!-- hide 'No Records Found.' in autodoc and show in component -->
                                <div style="text-align: center; display: none;" class="card_element_top_margin showImportant">
                                    <input type="checkbox" style="float: left; margin-left: 14px;"/>No Records Found.  <!-- US2543737 - Thanish - 5th Mar 2020 -->
                                </div>
                                <aura:set attribute="else">
                                    <tr>
                                        <td style="width:30px" class="autodoc">
                                            <input class="autodoc" type="checkbox" onclick="{!c.autodocClicked}"/>
                                        </td>
                                        <td>--</td>
                                        <td>--</td>
                                        <td>--</td>
                                        <td>--</td>
                                        <td>--</td>
                                        <td>--</td>
                                        <td>--</td>
                                        <td>--</td>
                                        <td style="cursor:pointer;"><a href="javascript:void(0)" onclick="{!c.showPHSContractDetails}">--</a></td>
                                        <td>--</td>
                                        <td>--</td>
                                        <td class="autodoc-case-item-resolved">
                                            <input class="autodoc-case-item-resolved" type="checkbox" onclick="{!c.resolveClicked}"/>
                                        </td>
                                    </tr>
                                </aura:set>
                            </aura:if>
                        </tbody>
                    </table>
                    <!-- show 'No Records Found.' in autodoc and hide in component -->
                    <aura:if isTrue="{!v.phsTableIsEmpty}">
                        <p class="hide" style="text-align: center; padding-top: 20px;">No Records Found.</p>
                    </aura:if>
                </div>
            </div>
            <!-- / PHS TAB -->
        </div>
        <!-- / TABS -->
    </div>
    <!-- </div> -->
    <!-- / Contract Summary -->
    
    <!-- Contract Details -->
    <aura:iteration items="{!v.contractDetailsList}" var="item">
        <div class="{!(item.hide) ? 'hideContractDetails' : ''}">
            <!-- C and S Contract Details -->
            <aura:if isTrue="{!item.type == 'CNS'}">
                <div class="card_top_margin"><c:ACET_CAndSContractDetails id="{!item.id}" parent="{!this}" contractDetails="{!item.data}" contractId="{!item.contractId}" effectiveDate="{!item.effectiveDate}" AutodocKey="{!v.AutodocKey + item.id}" autodocPageFeature="{!v.autodocPageFeature}"/></div>
            </aura:if>
            
            <!-- E and I Contract Details -->
            <aura:if isTrue="{!item.type == 'ENI'}">
                <div class="card_top_margin"><c:ACET_EAndIContractDetails id="{!item.id}" parent="{!this}" contractDetails="{!item.data}" contractId="{!item.contractId}" effectiveDate="{!item.effectiveDate}" AutodocKey="{!v.AutodocKey + item.id}" autodocPageFeature="{!v.autodocPageFeature}"/></div>
            </aura:if>
            
            <!-- M and R Contract Details -->
            <aura:if isTrue="{!item.type == 'MNR'}">
                <div class="card_top_margin"><c:ACET_MAndRContractDetails id="{!item.id}" parent="{!this}" contractDetails="{!item.data}" contractId="{!item.contractId}" effectiveDate="{!item.effectiveDate}" AutodocKey="{!v.AutodocKey + item.id}" autodocPageFeature="{!v.autodocPageFeature}"/></div>
            </aura:if>
            
            <!-- PHS Contract Details -->
            <aura:if isTrue="{!item.type == 'PHS'}">
                <div class="card_top_margin"><c:ACET_PHSContractDetails id="{!item.id}" contractId="{!item.contractId}"/></div>
            </aura:if>
        </div>
    </aura:iteration>
    <!-- / Contract Details -->
    
    </div>
</aura:component>