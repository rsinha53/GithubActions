<aura:component controller="ACET_ExplorePageCtrl" implements="force:appHostable,force:hasRecordId,flexipage:availableForAllPageTypes,lightning:isUrlAddressable"  access="global">
    <ltng:require styles="{!$Resource.ACETLink_MasterStyle}" />
    <aura:attribute name="yesOrNoOptions" type="List"/>
    <aura:attribute name="category" type="String" default="--"/>
    <aura:attribute name="reasonCode" type="String" default="--"/>
    <aura:attribute name="sendToListInputs" type="Object"/>
    <aura:attribute name="viewClaimsSubType" type="string" />
    <aura:attribute name="claimAutodocValues" type="List" />
    <aura:attribute name="IssueValue" type="string" />
    <aura:attribute name="stateOptions" type="List" />
    <aura:attribute name="typesValue" type="string" />
   <!--    <aura:attribute name="cashedValue" type="string" />-->
    <aura:attribute name="showError" type="boolean" default="false" />
    <aura:attribute name="IssueDropDown" type="List" />
    <aura:attribute name="reasonCodesList" type="Object"/>
    <aura:attribute name="reasonCodesMap" type="Map"/>
    <aura:attribute name="viewClaims" type="string" />
    <aura:attribute name="text" type="string" default="Add benefit/issue details in the comments box below."/>
    <aura:attribute name="noUnderLine" type="boolean" default="true" />
    <aura:attribute name="selectedUnresolvedClaims" type="List" access="public" default=""/>
    <aura:attribute name="isPaperType" type="Boolean" default="false" />
    <aura:attribute name="isStopapy" type="Boolean" default="false" />
    <aura:attribute name="isCashed" type="Boolean" default="false" />
    <aura:attribute name="isMethofDelValue" type="boolean" />
    <aura:attribute name="index" type="Integer" />
    <aura:attribute name="warning" type="string" default="" />
     <aura:attribute name="caseWrapper" type="Object" />
    <aura:attribute name="flowDetails" type="Object" access="public" />
    <aura:attribute name="contactNo" type="string" default="" />
    <aura:attribute name="typesOoptions" type="List" default="[
                                                              {'label': 'Electronic', 'value': 'Electronic'},
                                                              {'label': 'Paper', 'value': 'Paper'},
                                                              {'label': 'VCP', 'value': 'VCP'},
                                                              ]"/>
    <aura:attribute name="cashedOptions" type="List" default="[
                                                              {'label': 'Yes', 'value': 'Yes'},
                                                              {'label': 'No', 'value': 'No'},
                                                              ]"/>
     <aura:attribute name="addInRcdOptions" type="List" default="[
                                                              {'label': 'Anesthesia Time Units', 'value': 'Anesthesia Time Units'},{'label': 'COB Update', 'value': 'COB Update'},
                                                              {'label': 'Date of Service (DOS)', 'value': 'Date of Service (DOS)'},{'label': 'Description of Service', 'value': 'Description of Service'},
                                                              {'label': 'Diagnosis Code (Including 4th or 5th digit)', 'value': 'Diagnosis Code (Including 4th or 5th digit)'},{'label': 'EOB / PRA Requested', 'value': 'EOB / PRA Requested'},
                                                              {'label': 'Itemized Bill', 'value': 'Itemized Bill'},{'label': 'Medical Records', 'value': 'Medical Records'},
                                                              {'label': 'Member Name', 'value': 'Member Name'},{'label': 'Modifier', 'value': 'Modifier'},{'label': 'Place of Service (POS)', 'value': 'Place of Service (POS)'},
                                                              {'label': 'Proof of Timely Filing', 'value': 'Proof of Timely Filing'},{'label': 'Provider Information (Tax ID, Name etc.)', 'value': 'Provider Information (Tax ID, Name etc.)'},
                                                              {'label': 'Semi-private room rate', 'value': 'Semi-private room rate'},{'label': 'W-9', 'value': 'W-9'},{'label': 'Other', 'value': 'Other'},
                                                                 ]"/>
    <aura:attribute name="eReasonOptions" type="List" default="[
                                                               {'label': 'Member Eligible for Date of Service', 'value': 'Member Eligible for Date of Service'},
                                                               {'label': 'Newborn Eligible per Newborn Process', 'value': 'Newborn Eligible per Newborn Process'},
                                                               ]"/>
    <aura:attribute name="reasonOptions" type="List" default="[
                                                              {'label': 'Balance Billing Letter', 'value': 'Balance Billing Letter'},
                                                              {'label': 'EOB/PRA Request', 'value': 'EOB/PRA Request'},
                                                              {'label': 'Letter Request', 'value': 'Letter Request'},
                                                              ]"/>
    <aura:attribute name="MethofDelOptions" type="List" default="[
                                                                 {'label': 'Faxed', 'value': 'Faxed'},
                                                                 {'label': 'Mailed', 'value': 'Mailed'},
                                                                 ]"/>
    <aura:attribute name="sourceOptions" type="List" default="[
                                                              {'label': 'Call', 'value': 'Call'},
                                                              {'label': 'Link', 'value': 'Link'},
                                                              ]"/>
    <aura:attribute name="RODOptions" type="List" default="[
                                                                 {'label': 'Disputing Overpayment/Bulk Recovery', 'value': 'Disputing Overpayment/Bulk Recovery'},
                                                                 {'label': 'Submitted Check for Overpayment', 'value': 'Submitted Check for Overpayment'},
                                                                 ]"/>
    <aura:attribute name="policyOptions" type="List" default="[
                                                               {'label': 'Criteria Met to Allow Services', 'value': 'Criteria Met to Allow Services'},
                                                               {'label': 'No Policy Listed', 'value': 'No Policy Listed'},
                                                               ]"/>
    <aura:attribute name="psReasonOptions" type="List" default="[
                                                               {'label': 'Claim Pended Over TAT', 'value': 'Claim Pended Over TAT'},
                                                               ]"/>

                                                               
    <aura:attribute name="enableMemberInfoInput" type="Boolean" default="false" />
    <aura:attribute name="stopChngTab" type="string" default="" />
    <aura:attribute name="demographicInfo" type="Object" />
    <aura:registerEvent name="ClaimRoutingTabChangeEvent" type="c:ACET_ClaimRoutingTabChangeEvent"/>
    <aura:handler event="c:ACET_SubmitButtonValidation" action="{!c. validation}"/>
    <aura:handler name="init" value="{!this}" action="{!c.onLoad}" />
    <aura:method name="validation" action="{!c.validation}" />
    <aura:attribute name="isEnableContact" type="Boolean" default="false" />
    <aura:attribute name="isRequiredContact" type="Boolean" default="false" />
	<aura:attribute name="isFLN" type="Boolean" default="false" />
    <aura:attribute name="isEngine" type="Boolean" default="false" />																							   


    <aura:attribute name="availableTimeType" type="List" default="[{'label': 'AM', 'value': 'AM'},{'label': 'PM', 'value': 'PM'}]"/>
    <aura:attribute name="availableTimeZone" type="List" default="[{'label': 'Eastern - EST', 'value': 'Eastern - EST'},
                                                                  {'label': 'Central - CST', 'value': 'Central - CST'},
                                                                  {'label': 'Mountain - MST', 'value': 'Mountain - MST'},
                                                                  {'label': 'Pacific - PST', 'value': 'Pacific - PST'},
                                                                  {'label': 'Alaska - AKST', 'value': 'Alaska - AKST'},
                                                                  {'label': 'Hawaii - HST', 'value': 'Hawaii - HST'}
                                                                 ]"/>
    <aura:attribute name="conStartTime" type="String" default="" />
    <aura:attribute name="conStartType" type="String" default="" />
    <aura:attribute name="conEndTime" type="String" default="" />
    <aura:attribute name="conEndType" type="String" default="" />
    <aura:attribute name="conTimeZone" type="String" default="" />

    <!--US3463210 - Sravan-->
    <aura:attribute name="email" type="String" default=""/>
    <aura:attribute name="showEmail" type="Boolean" default="false"/>
    <aura:attribute name="adjustPadding" type="String" default=""/>
    <aura:attribute name="showClaimTable" type="Boolean" default="false"/>
    <aura:attribute name="policyType" type="String" default=""/>
    <aura:attribute name="claimProjectManagement" type="String" default=""/>
    <aura:attribute name="beginningDOS" type="Date" default=""/>
    <aura:attribute name="endingDOS" type="Date" default=""/>
    <aura:attribute name="showDos" type="Boolean" default="false"/>
    <aura:attribute name="enableIssue" type="Boolean" default="true"/>
    <aura:attribute name="enableBeginningDos" type="Boolean" default="true"/>
    <aura:attribute name="enableEndingDos" type="Boolean" default="true"/>
    <aura:attribute name="enableEmail" type="Boolean" default="true"/>
    <aura:attribute name="disableSubmit" type="Boolean" default="false"/>
	<aura:attribute name="mapClaimSummaryDetails" type="Object" default="[]"/>
    <aura:attribute name="TPMSLevel" type="String" default="" />


    <div  class="slds-card card_padding" style="border:1px solid rgba(221, 219, 218, 1)">
        <div >
            <span class="component_title ">Service Request Requirement</span>
        </div>
        <div aura:id="ServiceRequest" class="slds-grid slds-p-top_small card_bottom_margin">
            <div class="slds-col slds-size_2-of-12 slds-p-right_small"  >
                <lightning:combobox aura:id="Issue" name="Issue " label="Issue" placeholder="Select" options="{!v.IssueDropDown}" disabled="{!!v.enableIssue}" value="{!v.IssueValue}" onchange="{!c.onchngIssue}" />
            </div>
              <div class="slds-size_5-of-12" style=" padding-top: 4px; padding-right: 15px;">
                                 <div class="slds-size_5-of-7">
                                       <span style="color:grey;"><span style="color: rgb(194, 57, 52)">*</span>Hours of Operation</span>
                                 </div>
								 <div class="slds-grid">
                                 <div  class="slds-col">
                                    <div class="slds-size_6-of-7">
                                       <c:ComboboxField aura:id="startTimeId"
                                       fieldName=" "
                                       objectName="ACET_Hours_of_operation__mdt"
                                       searchFieldName="Time_Value__c"
                                       whereCondition=""
                                       orderBy="Order__c"
                                       selectedRecord="{!v.conStartTime}"
                                       searchKeyword="{!v.conStartTime}"
                                       isLabel= "false"/>
                                    </div>
                                 </div>
                                 <div  class="slds-col">
                                    <div class="slds-size_7-of-7">
                                       <div class="slds-grid">
                                          <div  class="slds-col">
                                             <lightning:combobox variant="label-hidden" required="true" class="slds-m-right_x-small" name="startTimeType" label="" value="{!v.conStartType}" options="{!v.availableTimeType }" />
                                          </div>
                                          <div  class="slds-col" style=" padding-top: 5px; padding-right: 5px;">
                                             To
                                          </div>
                                       </div>
                                    </div>
                                 </div>
                                 <div  class="slds-col">
                                    <div class="slds-size_6-of-7">
                                          <c:ComboboxField aura:id="endTimeId"
                                          fieldName=" "
                                          objectName="ACET_Hours_of_operation__mdt"
                                          searchFieldName="Time_Value__c"
                                          whereCondition=""
                                          orderBy="Order__c"
                                          selectedRecord="{!v.conEndTime}"
                                          searchKeyword="{!v.conEndTime}"
                                          isLabel= "false"/>
                                    </div>
                                 </div>
                                 <div  class="slds-col">
                                    <div class="slds-size_7-of-7">
                                          <lightning:combobox variant="label-hidden" required="true" class="slds-m-right_x-small" name="endTimeType" label="" value="{!v.conEndType}"  options="{!v.availableTimeType }" />
                                    </div>
                                 </div>
                                 <div  class="slds-col">
                                    <div class="slds-size_6-of-7">
                                          <lightning:combobox variant="label-hidden" required="true"  name="timeZone" label="" value="{!v.conTimeZone}"  options="{!v.availableTimeZone}"  />
                                    </div>
                                 </div>
                              </div>
							  </div>
           <div class="{!v.adjustPadding}" >
                <lightning:input aura:id="Contact"
                                 type="text"
                                 Name = "Contact Number"
                                 label= "Contact Number"                                 
                                 value="{!v.contactNo}"
                                 messageWhenValueMissing ="This field is required"
                                 required="{!v.isRequiredContact}"
                                 readonly="{!v.isEnableContact}"
                                 onchange="{!c.onChangeContact}" 
                                 onblur = "{!c.onLeaveContact}"/>
            </div>
            <!--US3463210 - Sravan - Start-->
            <aura:if isTrue="{!v.showEmail}">

            <div class="slds-col slds-size_2-of-12 slds-p-left_small slds-p-right_small" >
                <lightning:input aura:id="Email"
                                 type="email"
                                 Name = "Email"
                                 label= "Email"
                                 disabled="{!!v.enableEmail}"
                                 value="{!v.email}"
                                 onchange="{!c.onChangeOfValue}"
                                 required="true"/>
            </div>
            </aura:if>
            <!--US3463210 - Sravan - End-->
            <div class="slds-col slds-size_1-of-12 slds-p-left_small slds-p-right_small" >
                <div class="header-column">
                    <p class="field-title slds-form-element__label field_label_padding">Category</p>
                    <p class="valueCls" name="name">
                        <div class="slds-form-element__static">
                            <!--US3463210 - Sravan-->
                            <aura:if isTrue="{!!equals(v.policyType,'AP')}">
                            {!v.category}
                                <aura:set attribute="else">
                                        Claim Status
                                </aura:set>
                            </aura:if>
                        </div>
                    </p>
                </div>
            </div>
            <div class="slds-col slds-size_1-of-12 slds-p-left_small slds-p-right_small" >
                <div class="header-column">
                    <p class="field-title slds-form-element__label field_label_padding">Reason Code</p>
                    <p class="valueCls" name="name">
                        <div class="slds-form-element__static">
                            <aura:if isTrue="{!!equals(v.policyType,'AP')}">
                            {!v.reasonCode}
                                <aura:set attribute="else">
                                        Claims Inquiry
                                </aura:set>
                            </aura:if>
                        </div>
                    </p>
                </div>
            </div>
        </div>
        <!--US3463210 - Sravan  Start-->
        <aura:if isTrue="{!v.showDos}">
        <div class="slds-grid slds-wrap slds-p-top_large card_bottom_margin">
            <div class="slds-col slds-size_12-of-12 slds-p-left_small slds-p-right_small">
            	<lightning:formattedUrl value="{!v.claimProjectManagement}" label="Claim Project Management SOP"  target="_blank" />
            </div>
            <div class="slds-col slds-size_2-of-12 slds-p-top_large slds-p-left_small slds-p-right_small">
                Date Range for all Claims Affected.
            </div>
            <div class="slds-col slds-size_2-of-12 slds-p-left_small slds-p-right_small">
                <lightning:input aura:id="beginningDos" type="date" name="beginningDos" label="Beginning DOS" disabled="{!!v.enableBeginningDos}" value="{!v.beginningDOS}" onchange="{!c.onChangeOfValue}" required="true" dateStyle="short" pattern="/^([0-9]{4})\-([0-9]{2})\-([0-9]{2})$/" messageWhenBadInput="Error: Invalid Date" messageWhenPatternMismatch="Error: Invalid Date" autocomplete="off"/>
            </div>
            <div class="slds-col slds-size_2-of-12 slds-p-left_small slds-p-right_small">
                <lightning:input aura:id="endingDos" type="date" name="endingDos" label="Ending DOS" disabled="{!!v.enableEndingDos}" value="{!v.endingDOS}" onchange="{!c.onChangeOfValue}" required="true" dateStyle="short" pattern="/^([0-9]{4})\-([0-9]{2})\-([0-9]{2})$/" messageWhenBadInput="Error: Invalid Date" messageWhenPatternMismatch="Error: Invalid Date" autocomplete="off"/>
            </div>
        </div>
        </aura:if>
        <!--US3463210 - Sravan  End-->
        <div class="slds-grid slds-wrap slds-p-top_large card_bottom_margin">
             <aura:if  isTrue="{!v.showClaimTable}" > <!--US3463210 - Sravan -->
        <div class="slds-col slds-size_12-of-12 slds-has-flexi-truncate" style="width:35%">
                    <table class="slds-table slds-table_cell-buffer slds-no-row-hover slds-table_bordered claimsTbl ">
                        <thead>
                            <tr >
                                <th scope="col" style="width:260px">
                                    <div class="slds-truncate" title="">CLAIM #</div>
                                </th>
                                <th scope="col">
                                    <div class="slds-truncate" title="">SERVICE DATES</div>
                                </th>
								<aura:if  isTrue="{!v.isEngine}" >
                                 <th scope="col">
                                    <div class="slds-truncate" title="">PAY LOC/ENGINE</div>
                                </th>
                                </aura:if>
                                <aura:if  isTrue="{!v.isFLN }" >
                                 <th scope="col">
                                    <div class="slds-truncate" title="">FLN/DCC</div>
                                    </th></aura:if>																   
                            </tr>
                        </thead>
                        <tbody>
                            <aura:iteration items="{!v.selectedUnresolvedClaims}" var="claim" >
                                <tr style="height: 45px;">
                                    <td data-label="CLAIM #" class="slds-p-top_none" >
                                        <div class="slds-truncate slds-text-align_left" title="">{!claim.claimId}</div>
                                    </td>
                                    <td data-label="SERVICE DATES" class="slds-p-top_none" >
                                        <div class="slds-truncate slds-text-align_left" title="">{!claim.serviceDates}</div>
                                    </td>
                                    <aura:if  isTrue="{!v.isEngine}" >
                                        <td data-label="PAY LOC/ENGINE" class="slds-p-top_none" >
                                            <aura:if isTrue="{!not(empty(claim.engCode))}">
                                                <div class="slds-truncate slds-text-align_left" title="">{!claim.engCode}</div>
                                                <aura:set attribute="else">
                                                    <div class="slds-truncate slds-text-align_left" title="">--</div>
                                                </aura:set>
                                            </aura:if>
                                        </td>
                                    </aura:if>
                                    <aura:if  isTrue="{!v.isFLN }" >
                                        <td data-label="FLN/DCC" class="slds-p-top_none" >
                                            <aura:if isTrue="{!not(empty(claim.flndcc))}">
                                                <div class="slds-truncate slds-text-align_left" title="">{!claim.flndcc}</div>
                                                <aura:set attribute="else">
                                                    <div class="slds-truncate slds-text-align_left" title="">--</div>
                                                </aura:set>
                                            </aura:if>
                                        </td></aura:if>																	 
                                </tr>
                            </aura:iteration>
                        </tbody>
                    </table>
                    <p class="slds-text-heading_small slds-p-top_medium" style="font-weight: bold;"> {!v.text}</p>
            </div>
               </aura:if>
             <aura:if isTrue="{!equals(v.viewClaimsSubType,'Recovery, Overpayment Dispute')}">
                 <div class="slds-col slds-size_12-of-12 slds-has-flexi-truncate" style="width: 95%">
                    <table width="100%" class="slds-table slds-table_cell-buffer slds-no-row-hover slds-table_bordered claimsTbl " style="cursor:pointer;">
                        <thead>
                            <tr class="slds-text-heading--label">
                                <th scope="col">
                                    <div class="slds-truncate" title="">CLAIM #</div>
                                </th>
                                <th scope="col">
                                    <div class="slds-truncate" title="">SERVICE DATES</div>
                                </th>
                                 <th scope="col">
                                    <div class="slds-truncate" title="">REASON</div>
                                </th>
                                <th scope="col">
                                    <div class="slds-truncate" title="">CHECK #</div>
                                </th>
                                <th scope="col">
                                    <div class="slds-truncate" title="">DATE CHECK SENT</div>
                                </th>
                                <th scope="col">
                                    <div class="slds-truncate" title="">OVERPAYMENT REASON GIVEN</div>
                                </th>
                                <th scope="col">
                                    <div class="slds-truncate" title="">REASON FOR REVIEW BEING REQUESTED</div>
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <aura:iteration items="{!v.selectedUnresolvedClaims}" var="claim" indexVar="index" >
                                <tr onkeyup="{!c.getIndex}" onmouseover="{!c.getIndex}" data-index="{!index}">
                                    <td data-label="CLAIM #" class="slds-p-top_none"  >
                                        <div class="slds-truncate slds-text-align_left" title="">{!claim.claimId}</div>
                                    </td>
                                    <td data-label="SERVICE DATES" class="slds-p-top_none">
                                        <div class="slds-truncate slds-text-align_left" title="">{!claim.serviceDates}</div>
                                    </td>
                                    <td data-label="Reason" class="slds-p-top_none"  >
                                        <div  class="slds-text-align_left slds-var-m-bottom_large" title=""  >
                                            <lightning:combobox aura:id="RODReason"
                                                                name="RODReason"
                                                                placeholder="Select"
                                                                class="priorityAlign"
                                                                options="{!v.RODOptions}"
                                                                value="{!claim.RODreason}"
                                                                onchange="{!c.onchngReason}"
                                                                messageWhenValueMissing ="This field is required"
                                                                required="true"/>
                                        </div>
                                    </td>
                                    <td data-label="CHECK #" class="slds-p-top_none" style="width: 200px;">
                                        <div class="slds-text-align_left slds-var-m-bottom_large" title="">
                                            <lightning:input aura:id="CHECK#" messageWhenValueMissing ="This field is required" type="text" disabled="{!claim.disCheckField}" required="{! !claim.disCheckField}" Name ="CHECK #" value="{!claim.CheckValue}" onchange="{!c.onchngValue}"/>
                                        </div>
                                    </td>
                                    <td data-label="DATE CHECK SENT" class="slds-p-top_none" >
                                        <div class="slds-text-align_left slds-var-m-bottom_large" title="">
                                            <lightning:input aura:id="DATECHECKSENT" messageWhenValueMissing ="This field is required" dateStyle="short" messageWhenBadInput="Your entry does not match the allowed format mm/dd/yyyy." type="date" disabled="{!claim.disDateLField}" required="{! !claim.disDateLField}" Name ="DATE CHECK SENT" value="{!claim.DateValue}"  onchange="{!c.onchngValue}"/>
                                        </div>
                                    </td>
                                     <td data-label="OVERPAYMENT REASON GIVEN" class="slds-p-top_none" >
                                        <div class="slds-text-align_left slds-var-m-bottom_large" title="">
                                            <lightning:input aura:id="OVERPAYMENTREASONGIVEN" messageWhenValueMissing ="This field is required"  type="text" disabled="{!claim.disOverField}" required="{! !claim.disOverField}" Name ="OVERPAYMENT REASON GIVEN" value="{!claim.overPayValue}" onchange="{!c.onchngValue}"/>
                                        </div>
                                    </td>
                                    <td data-label="REASON FOR REVIEW BEING REQUESTED" class="slds-p-top_none" >
                                        <div class="slds-text-align_left slds-var-m-bottom_large" title="">
                                            <lightning:input aura:id="REASONFORREVIEWBEINGREQUESTED" messageWhenValueMissing ="This field is required" disabled="{!claim.disableReasonReview}"  required="{! !claim.disableReasonReview}" type="text" Name ="REASON FOR REVIEW BEING REQUESTED" value="{!claim.ReasonValue}" onchange="{!c.onchngValue}"/>
                                        </div>
                                    </td>
                                </tr>
                            </aura:iteration>
                        </tbody>
                    </table>
                 </div>
                </aura:if>
                <aura:if isTrue="{!equals(v.viewClaimsSubType,'Dispute Allowed Amount')}">
                 <div class="slds-col slds-size_12-of-12 slds-has-flexi-truncate" style="width: 90%">
                    <table width="100%" class="slds-table slds-table_cell-buffer slds-no-row-hover slds-table_bordered claimsTbl " style="cursor:pointer;">
                        <thead>
                            <tr class="slds-text-heading--label">
                                <th scope="col">
                                    <div class="slds-truncate" title="">CLAIM #</div>
                                </th>
                                <th scope="col">
                                    <div class="slds-truncate" title="">SERVICE DATES</div>
                                </th>
                                 <th scope="col">
                                    <div class="slds-truncate" title="">PROVIDER STATUS</div>
                                </th>
                                <th scope="col">
                                    <div class="slds-truncate" title="">BENEFIT LEVEL</div>
                                </th>
                                <th scope="col">
                                    <div class="slds-truncate" title="">EXPECTED ALLOWED $</div>
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <aura:iteration items="{!v.selectedUnresolvedClaims}" var="claim" indexVar="index" >
                                <tr onkeyup="{!c.getIndex}" onmouseover="{!c.getIndex}" data-index="{!index}">
                                    <td data-label="CLAIM #" class="slds-p-top_none"  >
                                        <div class="slds-truncate slds-text-align_left" title="">{!claim.claimId}</div>
