<aura:component >
    <aura:attribute name="rxDetails" type="ADBMemberDetailWrapper" />
    <div>
        Rx
    </div>
    <div class="slds-p-left_small">
        <div  class="slds-form-element slds-form-element_readonly">
            <span class="slds-form-element__label">Carrier:</span>
            <div class="slds-form-element__control">
                <div class="slds-form-element__static">{!v.rxDetails.carrierId}</div>
            </div>
        </div>
        <div  class="slds-form-element slds-form-element_readonly">
            <span class="slds-form-element__label">Account:</span>
            <div class="slds-form-element__control">
                <div class="slds-form-element__static">{!v.rxDetails.accountId}</div>
            </div>
        </div>
        <div  class="slds-form-element slds-form-element_readonly">
            <span class="slds-form-element__label">Group:</span>
            <div class="slds-form-element__control">
                <div class="slds-form-element__static">{!v.rxDetails.groupId}</div>
            </div>
        </div>
        <div  class="slds-form-element slds-form-element_readonly">
            <span class="slds-form-element__label">PBM:</span>
            <div class="slds-form-element__control">
                <div class="slds-form-element__static">{!v.rxDetails.pbm}</div>
            </div>
        </div>
        <div  class="slds-form-element slds-form-element_readonly">
            <span class="slds-form-element__label">Effective Date:</span>
            <div class="slds-form-element__control">
                <div class="slds-form-element__static">{!v.rxDetails.fromDate}</div>
            </div>
        </div>
        <div  class="slds-form-element slds-form-element_readonly">
            <span class="slds-form-element__label">End Date:</span>
            <div class="slds-form-element__control">
                <div class="slds-form-element__static">{!v.rxDetails.thruDate}</div>
            </div>
        </div>
        
    </div>
</aura:component>