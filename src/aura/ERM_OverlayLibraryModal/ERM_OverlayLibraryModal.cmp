<aura:component access="global">
    <lightning:overlayLibrary aura:id="overlayLib"/>
	<aura:registerEvent name="appEvent" type="c:ERM_OverlayLibraryModalEvent"/>
    <!--Component Start--> 
    <div class="slds-text-heading_small">I attest that all information submitted in this request has 
        been validated against source data and reviewed for errors</div>
    
    <br/><br/>
    <div class="slds-text-align_center slds-m-around_x-medium">
        <lightning:button name="cancel" label="Cancel" onclick="{!c.handleCancel}"/>
        <lightning:button name="ok" label="OK" variant="brand" onclick="{!c.handleOK}"/>
    </div>
    <!--Component End-->
</aura:component>