                        <aura:if isTrue="{!v.isNewContactModal}"> 
                            <lightning:button variant="brand"
                                              label="Save"
                                              onclick="{!c.submitAddContactDetails}"/>
                        </aura:if>
                        <aura:if isTrue="{!v.isEditContactModal}"> 
                            <lightning:button variant="brand"
                                              label="Save"
                                              onclick="{!c.submitEditContactDetails}"/>
                        </aura:if>
                    </footer>
                </div>
            </section>
            <div class="slds-backdrop slds-backdrop_open"></div>
        </aura:if>
    </div>
</aura:component>