<aura:component implements="flexipage:availableForAllPageTypes,forceCommunity:availableForAllPageTypes,forceCommunity:layout,forceCommunity:themeLayout" access="global" controller="SNI_FL_CACredirectURL" >
    <div class="slds-size — 1-of-1 slds-medium-size — 1-of-2 slds-large-size — 1-of-4">
    <div class="slds slds-theme_default"> 
        <div class=" Loginthemelayout">
            <div class=" welcomeFormStyleLayout">  
                    <lightning:layout>                        
                        <lightning:layoutItem class="slds-border_right welcomeFormStyle " padding="around-small">
                          <div class="IpadBackImg">
                                    <div class = " IpadBackImg">
                                        <img class="IpadBackImg" src="{!($Resource.CACMobileImage)}"/>
                                    </div>
                        </div>
                            <div class="custom-box leftSideSpacing">
                                <div class="logoSpacing">
                                    <img src="{!$Resource.CAConnectLogo}"/>
                                </div>
                                <div class="custom-box mobileCenterAlign">
                                    <!-- <div class="fllogomedia">  
                                         <img src="{!$Resource.CAConnectLogo}"/>
                                            </div> -->
                                    <div class="MobileBackImg">
                                        <div class = "slds-align_absolute-center MobileBackImg">
                                            <img class="MobileBackImg" src="{!($Resource.CACMobileImage)}"/>
                                        </div>
                                    </div>
                                <div class="custom-box mobileCenterAlign">
                                <div class="slds-text-longform divstyle spacing-left-text">
                                    <div class="spacing-left-text">
                                        <h2 class="slds-text-heading_small welcome-text h4 welcome-text-header">Patients/Members</h2>
                                       <b> <ui:button class="sfdc_button" aura:id="submitButton1" press="{!c.handleLogin}" label="Login" /> </b>
                                    </div>                            
                                    <div class="spacing-left-text">
                                        <h2 class="slds-text-heading_small  welcome-text">New to Care Advisor Connect?</h2>
                                        <b><ui:button class="sfdc_button1" aura:id="submitButton" press="{!c.handleRegister}" label="Register" /></b>
                                        
                                    </div>  
                                    <hr class="Linealign" style="height:1px;"/>
                                    <div class="spacing-left-text">
                                        <h2 class="slds-text-heading_small  welcome-text">Caregivers</h2>
                                      <b>  <ui:button class="sfdc_button" aura:id="submitButton" press="{!c.handleCaregiverLogin}" label="Login" /> </b>
                                    </div>
                                    </div> 
                                </div>
                            </div>
                            </div>
                        </lightning:layoutItem>
                        <lightning:layoutItem class="loginFormStyle mobilescreen" flexibility="auto" padding="around-large">
                            
                            <div class="BackImg">
                              <!--  <div class = "slds-align_absolute-center BackImg">-->
                                    <img  src="{!($Resource.CACBackgroundImage)}"/>
                                <!--</div>-->
                            </div>
                        </lightning:layoutItem>
                        
                    </lightning:layout></div>
                
            </div> 
        </div>
    </div>
    <!-- </div> -->
    <!-- </div> -->
    
</aura:component>