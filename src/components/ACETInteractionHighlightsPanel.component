    </apex:actionFunction>
    <apex:actionFunction name="showHoldCodeHLP" action="{!showHoldCodeHLP}" reRender="alertHoldCodeHLP,holdLoading" oncomplete="showMaskHLP();resetTable();" status="HoldStatusHLP"/>
    <apex:actionFunction name="hideHoldCodeHLP" action="{!hideHoldCodeHLP}" reRender="alertHoldCodeHLP,holdLoading" oncomplete="hideMaskHLP()"/>
