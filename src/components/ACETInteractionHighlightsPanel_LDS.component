<apex:outputPanel id="alertModalWindow" >
    <div class ="slds-scope" style="background-color:white">
        <apex:outputPanel style="display:{!IF(NOT(isShowALert),'','none')}" rendered="{!NOT(ISNULL(Subject))}"  >
