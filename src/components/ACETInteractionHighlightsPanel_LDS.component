                                   <apex:outputText value="{!mAl.Benefit_Bundle_Option_ID__c}" style="vertical-align:text-top;word-wrap: break-word;white-space: normal;width:7%;" />
                                </apex:column>    -->                        
                                <apex:column headerValue="  ID #"   value="{!mAl.ID_Number__c}" style="vertical-align:Top;align:center; word-break: break-all;white-space: normal;width:14%;" />
                                <apex:column headerValue="TITLE" value="{!mAl.Alert_Title__c }" style="vertical-align:text-top;word-wrap: break-word;white-space: normal;width:16%;" />
                                <apex:column headerValue="COMMENTS" value="{!mAl.Alert_Comments__c}" style="vertical-align:Top;word-wrap: break-word;white-space: normal;width:36%;"/>
                                <apex:column headerValue="EFFECTIVE" value="{!mAl.Effective_Date__c}" style="vertical-align:Top;word-wrap: break-word;white-space: normal;width:1%;" />
                                <apex:column headerValue="END" value="{!mAl.End_Date__c}" style="vertical-align:Top;word-wrap: break-word;white-space: normal;width:1%;" />
