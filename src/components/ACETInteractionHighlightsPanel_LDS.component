<apex:component controller="ACETInteractionHighlightsPanelController"
    allowDML="true"   id="theHighLightPanel">
    <apex:attribute type="Interaction__c" name="InteractionAtt"
        description="Interaction record created for provider or member flow"
        assignTo="{!Interaction}" required="false" />   
    <apex:attribute type="Contact" name="SubjectAtt"
        description="subject record associated with the flow"
        assignTo="{!Subject}" required="false" />
    <apex:attribute type="Alert__c[]" name="TotalAlerts"
        description="total  Alerts related to the Member"
        assignTo="{!lstMemAlertOtherPage}" required="false" />
    <apex:attribute type="Alert__c[]" name="TotalProviderAlerts"
        description="total  Alerts related to the Provider"
        assignTo="{!lstProviderAlertDetailPage}" required="false" />  
    <apex:attribute type="Alert__c[]" name="TotalProducerAlerts"
        description="total  Alerts related to the Producer"
        assignTo="{!lstProducerAlertDetailPage}" required="false" />
     <apex:attribute type="Alert__c[]" name="TotalGroupAlerts"
        description="total  Alerts related to the Group"
        assignTo="{!lstGroupAlertDetailPage}" required="false" />
    <apex:attribute type="ACETHighlightsPanelAdditionalInfo"
        name="AdditionalInfoAtt"
        description="other fields displayed in panel from oil"
        assignTo="{!AdditionalInfo}" required="false" />
    <apex:attribute type="String" name="SurrogateKey"
        description="SurrogateKey of the Member" required="false" />
    <apex:attribute type="ACETGroupDetail" name="GroupDetailAtt"
        description="Group Details" assignTo="{!groupDetails}"
    required="false" />
    <apex:attribute type="String" name="groupNameAtt"
    description="Group Name" assignTo="{!groupName}"
    required="false" />
    
    <apex:attribute type="Boolean" name="showALertAtt"
        description="Show the Group Alert" assignTo="{!isshowALert}" required="false" id="showgroupAlert"/>
    <apex:attribute type="String" name="selectedOriginator"
            description="selected originator on member detail page"
            required="false" id="seledtedOriginator" />

    <apex:attribute type="String" name="sniEligibility"
                    description="special Needs SNI Eligibility status"
                    required="false" id="sniEligibility" />        
    
    <!--<apex:includeScript value="{!URLFOR($Resource.ACETUIResources, '/JS/bootstrap.min.js')}"/> -->

    <script type="text/javascript">
        var alertModalWindow = $("[id$='thePage:frmid:popupPanel:theHighLightPanel:alertModalWindow']");
        var alertGroupModalWindow = $("[id$='alertGroupModalWindow']");
        
        var toggleHighlightsPanel = function()  {
            $(".highlights-panel-collapsible").slideToggle(400, function(){
            
                if ($(".panel-arrow-down").hasClass("show-arrow"))  {
                    $(".panel-arrow-down").addClass("hide-arrow").removeClass("show-arrow");
                    $(".panel-arrow-up").addClass("show-arrow").removeClass("hide-arrow");
                    $(".highlights-panel-container").css({"border-bottom":"2px solid #ccc", "border-top":"none"});
                } else  {
                    $(".panel-arrow-down").removeClass("hide-arrow").addClass("show-arrow");
                    $(".panel-arrow-up").removeClass("show-arrow").addClass("hide-arrow");
                    $(".highlights-panel-container").css({"border-top":"2px solid #ccc", "border-bottom":"none"});
                }
            });
        };
         
        $(document).ready(function() {
            $("[id$='interactionId']").on('click', function(){
                //console.log('Clicked Interaction...');       
                sforce.console.getEnclosingPrimaryTabId(function(result) {
                    sforce.console.openSubtab(result.id, '/'+'{!Interaction.Id}', true, '{!Interaction.Name}', null);
                });
                return false;
            });
            getAllAlerts();
            //console.log('-----------InConsole----------->{!AdditionalInfo}'); 
        });      
     
        function renderHippaLabel() {
          //console.log('{!JSENCODE(selectedOriginator)}@@ ' + '{!JSENCODE(selectedOriginator)}' + ' @@{!JSENCODE(Interaction.Originator_Type__c)}@@ ' + '{!JSENCODE(Interaction.Originator_Type__c)}');
            //For HIPPA Disclosure
            if (("{!JSENCODE(selectedOriginator)}" == null || "{!JSENCODE(selectedOriginator)}" == "None" 
                || "{!JSENCODE(selectedOriginator)}" == "")  && "{!JSENCODE(Interaction.Originator_Type__c)}" == "Member")  {
                $("[id$='hippaPanel']").css("display","none");
            } else  {
                $("[id$='hippaPanel']").css("display","block");
            }
            
            var onM = '<svg id = "svgon" style = "margin-bottom:-1%;fill:{!JSENCODE($Label.ACETHippaHelpIconColorCode)};" version="1.1" xmlns="http://www.w3.org/2000/svg" width="14" height="18" viewBox="0 0 24 28"><path d="M14 21.5v-3c0-0.281-0.219-0.5-0.5-0.5h-3c-0.281 0-0.5 0.219-0.5 0.5v3c0 0.281 0.219 0.5 0.5 0.5h3c0.281 0 0.5-0.219 0.5-0.5zM18 11c0-2.859-3-5-5.688-5-2.547 0-4.453 1.094-5.797 3.328-0.141 0.219-0.078 0.5 0.125 0.656l2.063 1.563c0.078 0.063 0.187 0.094 0.297 0.094 0.141 0 0.297-0.063 0.391-0.187 0.734-0.938 1.047-1.219 1.344-1.437 0.266-0.187 0.781-0.375 1.344-0.375 1 0 1.922 0.641 1.922 1.328 0 0.812-0.422 1.219-1.375 1.656-1.109 0.5-2.625 1.797-2.625 3.313v0.562c0 0.281 0.219 0.5 0.5 0.5h3c0.281 0 0.5-0.219 0.5-0.5v0c0-0.359 0.453-1.125 1.188-1.547 1.188-0.672 2.812-1.578 2.812-3.953zM24 14c0 6.625-5.375 12-12 12s-12-5.375-12-12 5.375-12 12-12 12 5.375 12 12z"></path></svg>';

            $("[id$='hippaLbl']").find(".spanhippaIco").html(onM);
            $("[id$='hippaLbl']").css("cursor","pointer");
            $(".spanhippaIco").mouseover(function() {
                if($("#hippaMsgBox").css("display") == 'none')  {
                    getCategory();  //this action function is defined in the HIPPA Disclosure Component
                    $(".spanhippaIco").html(onM);
                    $("#hippaMsgBox").css("display", "block");
                }
            });
            //For HIPPA Disclosure ends 
            
            $(".onshore-indicator").mousemove(function()    {
                var tooltip = $(this).parent().find(".tooltip-modal");
                $(tooltip).css("display","block");
                var posX = event.clientX + 'px';
                var posY = event.clientY + 'px';
                $(tooltip).css("top",posY);                
                $(tooltip).css("left",posX);                                
            
            });
            
            $(".onshore-indicator").mouseout(function() {
                var tooltip = $(this).parent().find(".tooltip-modal");
                $(tooltip).css("display","none");
            });
        }     
    
        function getProducerAlertjs()   {
             var alertProdSize = "{!lstProducerAlertDetailPage.size}";
             if (alertProdSize == 0 || alertProdSize == '') {
                 //console.log('inside the Producer alert');
                 getProducerAlert();
             }    
        } 
       
        function getGroupAlertjs()  {
             var alertGroupSize = "{!lstGroupAlertDetailPage.size}";
             if (alertGroupSize == 0 || alertGroupSize == '')   {
                 //console.log('inside the Group alert');
                 getGroupAlert();
             }    
        }
        
        function getMemberAlertjs() {
            var alertListSize = "{!lstMemAlertOtherPage.size}";
            if (alertListSize == 0 || alertListSize == '')  {
                //console.log('inside the alert');
                getMemberAlert();
            }    
        }
        
        function getProviderALertjs()   {
             var alertProviderSize = "{!lstProviderAlertDetailPage.size}";
             if (alertProviderSize  == 0 || alertProviderSize == '')    {
             //console.log('alertProviderSize '+alertProviderSize );
                 getProviderAlert();
             }    
        }
        
        function closealertModalWindow()    {
            $("[id$='alertModalWindow']").css("display","none");
            $("[id$='alertProviderModalWindow']").css("display","none");    
            $("[id$='alertGroupModalWindow']").css("display","none");
            $("[id$='alertProducerModalWindow']").css("display","none");
            rerenderalertModalWindow();
        } 
         
        function OpenInteractionSubTab(RecordId, RecordName){
            sforce.console.getEnclosingPrimaryTabId(function(result) {
                  sforce.console.openSubtab(result.id, '/'+RecordId, true,RecordName, null);
              });
              return false;
        }
        function openCirrusWindow(CIRRUSURL) {
            console.log('CIRRUSURL::'+CIRRUSURL);
            window.open(CIRRUSURL, 'CIRRUS', 'toolbars=0,width=1200,height=800,left=0,top=0,scrollbars=1,resizable=1'); 
            
        }
    </script>
