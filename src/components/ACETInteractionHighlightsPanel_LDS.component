                        </div>  
                    </div>
                    <apex:outputPanel style="width:100%">
                        <div class=" slds-table slds-table_bordered slds-table_cell-buffer slds-text-title slds-table_fixed-layout" columns="1" >
                            <apex:dataTable value="{!if(lstMemAlert==null,lstMemAlertOtherPage,lstMemAlert)}" var="mAl" rendered="{!lstMemAlert.size != 0}" style="width:100%;">
                                <apex:column headerValue="TYPE" value="{!mAl.Type__c}" style="vertical-align:Top; word-wrap: break-word;white-space: normal;width:10%;" />                              
                                <apex:column headerValue="GROUP #"  value="{!mAl.Group_Number__c}" style="vertical-align:Top; word-wrap: break-word;white-space: normal;width:11%;" />
                                <apex:column headerValue="BBO ID"  value="{!mAl.Benefit_Bundle_Option_ID__c}" style="width:13%; vertical-align:Top; word-wrap: break-word;white-space: normal;" />
