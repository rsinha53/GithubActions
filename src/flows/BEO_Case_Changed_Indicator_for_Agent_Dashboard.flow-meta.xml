AND(

ISCHANGED([Case].Status ),
ISPICKVAL([Case].Case_Type__c , &apos;BEO Case&apos;), 
[Case].LastModifiedById != [Case].OwnerId 

     ),
AND(

ISCHANGED([Case].OwnerId),
ISPICKVAL([Case].Case_Type__c , &apos;BEO Case&apos;)
)
   )</stringValue>
