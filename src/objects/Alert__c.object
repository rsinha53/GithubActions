    <validationRules>
        <fullName>Alert_Comments_cannot_be_Blank</fullName>
        <active>false</active>
        <description>Does not allow user to save an alert if Alert Comment is BLANK</description>
        <errorConditionFormula>ISBLANK( Alert_Comments__c )</errorConditionFormula>
        <errorDisplayField>Alert_Comments__c</errorDisplayField>
        <errorMessage>You must enter Alert Comments in order to save this record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Benefit_Bundle_Option_ID_GroupAndBundle</fullName>
        <active>true</active>
        <description>Benefit Bundle Option ID and Group ID or ID Number can&apos;t be added at the same time</description>
        <errorConditionFormula>IF(AND(ISPICKVAL(Type__c ,&apos;Member Group&apos;),NOT(ISBLANK(Benefit_Bundle_Option_ID__c)),OR(NOT(ISBLANK(Group_Number__c)),NOT(ISBLANK(ID_Number__c)))),true,false)</errorConditionFormula>
        <errorDisplayField>Benefit_Bundle_Option_ID__c</errorDisplayField>
        <errorMessage>You cannot enter Group Number or ID Number with Benefit Bundle Option ID.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Benefit_Bundle_Option_ID_NotMemberGroup</fullName>
        <active>true</active>
        <description>type selected is different than Member Group and Benefit Bundle Option ID is not allowed</description>
        <errorConditionFormula>IF(AND(NOT(ISPICKVAL(Type__c ,&apos;Member Group&apos;)),NOT(ISBLANK(Benefit_Bundle_Option_ID__c))),true,false)</errorConditionFormula>
        <errorDisplayField>Benefit_Bundle_Option_ID__c</errorDisplayField>
        <errorMessage>Benefit Bundle Option ID is only entered for Member Group Alerts.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bundle_Id_Required</fullName>
        <active>true</active>
        <description>Check if bundle option id is blank when type is member group and group number is blank</description>
        <errorConditionFormula>IF(AND(ISPICKVAL( Type__c , &apos;Member Group&apos;), ISBLANK( Group_Number__c),ISBLANK(Benefit_Bundle_Option_ID__c) ),true,false)</errorConditionFormula>
        <errorDisplayField>Benefit_Bundle_Option_ID__c</errorDisplayField>
        <errorMessage>You must enter either the Benefit Bundle Option ID or the Group Number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_cannot_be_before_Effective_Date</fullName>
        <active>true</active>
        <errorConditionFormula>End_Date__c &lt;  Effective_Date__c</errorConditionFormula>
        <errorMessage>Effective Date must be the same as or before End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Group_Number_Required</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(AND(OR(ISPICKVAL( Type__c , &apos;Member Group&apos;),ISPICKVAL( Type__c , &apos;Provider Group&apos;)), ISBLANK( Group_Number__c)),AND(ISPICKVAL( Type__c , &apos;Member Group&apos;), ISBLANK( Group_Number__c),ISBLANK(Benefit_Bundle_Option_ID__c) )),true,false)</errorConditionFormula>
        <errorDisplayField>Group_Number__c</errorDisplayField>
        <errorMessage>Group Number  is required when the type is Member Group or Provider Group</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ID_Number_Required</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(OR(ISPICKVAL( Type__c , &apos;Member&apos;),ISPICKVAL( Type__c , &apos;Provider&apos;),ISPICKVAL( Type__c , &apos;Producer&apos;)),  ISBLANK(ID_Number__c) ),true,false)</errorConditionFormula>
        <errorDisplayField>ID_Number__c</errorDisplayField>
        <errorMessage>ID number is required when the type is Member or Provider or Producer</errorMessage>
    </validationRules>
