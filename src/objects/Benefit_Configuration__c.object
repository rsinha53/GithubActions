    <validationRules>
        <fullName>Category_Reqd</fullName>
        <active>true</active>
        <description>Ensures that a Category Keyword is entered if Category is included in Benefit Group Type</description>
        <errorConditionFormula>AND(
INCLUDES(Benefit_Grouping_Type__c , &quot;Category&quot;),
ISBLANK(  Category_Keyword__c  )
)</errorConditionFormula>
        <errorDisplayField>Category_Keyword__c</errorDisplayField>
        <errorMessage>Category field must be filled out for Category grouping type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Specialty_Reqd</fullName>
        <active>true</active>
        <description>Ensures that a Specialty Keyword is entered if Specialty is included in Benefit Group Type</description>
        <errorConditionFormula>AND(
INCLUDES(Benefit_Grouping_Type__c , &quot;Specialty&quot;),
ISBLANK(  Specialty_Keyword__c  )
)</errorConditionFormula>
        <errorDisplayField>Specialty_Keyword__c</errorDisplayField>
        <errorMessage>NDB Code and Specialty fields must be filled out for Specialty grouping type</errorMessage>
    </validationRules>
