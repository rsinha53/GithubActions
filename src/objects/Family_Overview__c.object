    <validationRules>
        <fullName>Designation_check</fullName>
        <active>true</active>
        <description>SENS need to provide an error when Advisor try to set the designation to deferred or dormant</description>
        <errorConditionFormula>AND(
OR(ISPICKVAL(Designation__c,&apos;Dormant&apos;),ISPICKVAL(Designation__c,&apos;Deferred&apos;)),$Setup.fo_validations__c.Is_Automation__c  &lt;&gt;  true, ISCHANGED(Designation__c) )</errorConditionFormula>
        <errorDisplayField>Designation__c</errorDisplayField>
        <errorMessage>Please choose another designation option as Deferred or Dormant are not available for manual selection.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Family_Overview_Name_Account_read_only</fullName>
        <active>true</active>
        <description>Makes the Family Overview and Account read only for Family Engagement Center profile</description>
        <errorConditionFormula>OR(AND( ISCHANGED( Name ) , $User.Profile_Name__c = &apos;Family Engagement Center&apos;), AND( ISCHANGED(  Account__c  ) , $User.Profile_Name__c = &apos;Center for Health Navigation&apos;))</errorConditionFormula>
        <errorMessage>You do not have permission to edit the Family Overview Name or Account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>First_Contact_into_Center_validation</fullName>
        <active>true</active>
        <description>You cannot edit this field when Digital Onboard is First Contact into Center.</description>
        <errorConditionFormula>AND( ISCHANGED(First_Family_Contact_Into_Center__c),ISPICKVAL( PRIORVALUE(First_Family_Contact_Into_Center__c) ,&apos;Digital Onboard&apos;))</errorConditionFormula>
        <errorDisplayField>First_Family_Contact_Into_Center__c</errorDisplayField>
        <errorMessage>You cannot edit this field when Digital Onboard is First Contact into Center.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Populate_Exception</fullName>
        <active>true</active>
        <description>Preventing None from being selected</description>
        <errorConditionFormula>ISPICKVAL(Exception__c,&apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Exception__c</errorDisplayField>
        <errorMessage>Please select a value.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
