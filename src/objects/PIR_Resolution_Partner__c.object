    <fields>
        <fullName>Resolution_Partner_Outcome__c</fullName>
        <description>#US2853220
Indicate whether the issue has been resolved by the resolution partner, or is still unresolved and must be sent back.</description>
        <externalId>false</externalId>
        <label>Resolution Partner Outcome</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Incorrect Route</fullName>
                    <default>false</default>
                    <label>Incorrect Route</label>
                </value>
                <value>
                    <fullName>Must Resend</fullName>
                    <default>false</default>
                    <label>Must Resend</label>
                </value>
                <value>
                    <fullName>Open</fullName>
                    <default>false</default>
                    <label>Open</label>
                </value>
                <value>
                    <fullName>Resolved</fullName>
                    <default>false</default>
                    <label>Resolved</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>SLA_Completion_Date__c</fullName>
        <description>#US2853220
Date when they resolve this resolution partner</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <label>SLA Completion Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
