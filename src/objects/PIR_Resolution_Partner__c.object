    </fields>
    <fields>
        <fullName>SLA_Due_Date__c</fullName>
        <description>SLA Due Date of the Resolution Partner.</description>
        <externalId>false</externalId>
        <formula>SLA_Routed_Date__c+Provider_Matrix_Reactive__r.Resolution_Partner_SLA__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>SLA Due Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>SLA_Indicator__c</fullName>
        <description>Indicate icon based on SLA

Yellow icon = Open SLA
Green icon = Completed SLA
Red icon = SLA needs attention</description>
        <externalId>false</externalId>
        <formula>IF(
(!ISBLANK( SLA_Completion_Date__c ) &amp;&amp; SLA_Completion_Date__c &lt; SLA_Due_Date__c &amp;&amp;  TEXT(Resolution_Partner_Outcome__c ) != &apos;Open&apos; ),
IMAGE(&quot;/img/msg_icons/confirm32.png&quot;, &quot;Status Indicator&quot;, 30, 30),
IF((SLA_Due_Date__c &gt; (TODAY()) &amp;&amp; TEXT(Resolution_Partner_Outcome__c ) == &apos;Open&apos;),
IMAGE(&quot;/img/msg_icons/warning32.png&quot;, &quot;Status Indicator&quot;, 30, 30),
IMAGE(&quot;/img/msg_icons/error32.png&quot;, &quot;Status Indicator&quot;, 30, 30)
)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Yellow icon = Open SLA
Green icon = Completed SLA
Red icon = SLA needs attention</inlineHelpText>
        <label>SLA Indicator</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>SLA_Routed_Date__c</fullName>
        <description>#US2853220
Date when they route it to the resolution partner</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <label>SLA Routed Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
