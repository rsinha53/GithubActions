    <validationRules>
        <fullName>Prevent_RP_Creation_while_case_is_closed</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
 OR(ISNEW(), ISCLONE() ),
 TEXT( Case__r.Status ) == &apos;Closed&apos;
)</errorConditionFormula>
        <errorMessage>RP record cannot be created/cloned in closed case</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_RP_Edit_while_case_is_closed</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
 NOT(ISCHANGED( Case__c ))  ,
 TEXT( Case__r.Status ) == &apos;Closed&apos;
)</errorConditionFormula>
        <errorMessage>RP record cannot be edited  in closed case</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Proactive_RP_Creation_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
 ISNEW(),RecordType.DeveloperName =&apos;Proactive_Action_Partner&apos;,
 ISBLANK( Provider_Matrix_Proactive__c ) 
)</errorConditionFormula>
        <errorDisplayField>Provider_Matrix_Proactive__c</errorDisplayField>
        <errorMessage>Please provide mandatory field: Internal Business Partner</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reactive_RP_Creation_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
 ISNEW(),RecordType.DeveloperName =&apos;Reactive_Resolution_Partners&apos;,
 OR(
ISBLANK( Provider_Matrix_Reactive__c),
 ISBLANK(SLA_Routed_Date__c),
 ISBLANK(TEXT( Resolution_Partner_Outcome__c ))
)
)</errorConditionFormula>
        <errorMessage>Please provide mandatory field: Resolution Partners Name / Resolution Partner Outcome/ SLA Routed Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SLA_Completion_Date_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
RecordType.DeveloperName = &quot;Reactive_Resolution_Partners&quot;,
ISPICKVAL( Resolution_Partner_Outcome__c , &apos;Resolved&apos;),
OR(
ISBLANK( SLA_Completion_Date__c ),ISBLANK(SLA_Routed_Date__c)
)
)</errorConditionFormula>
        <errorMessage>SLA Completion Date &amp; SLA Routed Date should not be blank when closing RP.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
