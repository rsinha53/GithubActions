    <validationRules>
        <fullName>Check_More_than_one_Project_Submission</fullName>
        <active>true</active>
        <description>User must not create more than one project submission details for one case.</description>
        <errorConditionFormula>AND(
  NOT($Setup.ACETConditionalAccess__c.isSkipRESCValidation__c),
  ISNEW(), 
  Case__r.Count_of_Project_Submission__c &gt; 0
)</errorConditionFormula>
        <errorMessage>You cannot create multiple project submission details for one case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Root_Cause_Not_Remediated_Reason</fullName>
        <active>true</active>
        <description>This rule forces to provide reason for Root Cause not remediated.</description>
