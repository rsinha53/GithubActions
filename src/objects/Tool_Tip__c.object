    <deploymentStatus>Deployed</deploymentStatus>
    <description>Object to store business enabled contextual tips based on elements on the screen (e.g, guidance on a claim denial by its denial remark code).</description>
    <enableActivities>false</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>false</enableFeeds>
    <enableHistory>true</enableHistory>
    <enableLicensing>false</enableLicensing>
    <enableReports>true</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>Private</externalSharingModel>
    <fields>
        <fullName>SOP_or_Document_Link__c</fullName>
        <description>Holds the URL of the SOP or document</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <inlineHelpText>Enter the URL of the SOP/Document</inlineHelpText>
        <label>SOP or Document Link</label>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Url</type>
    </fields>
    <fields>
        <fullName>SOP_or_Document_Name__c</fullName>
        <description>Holds the name of a linked SOP or document</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <inlineHelpText>Enter the name of the SOP/Document</inlineHelpText>
        <label>SOP or Document Name</label>
        <length>255</length>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Tip__c</fullName>
        <description>Holds the tip for the remark code or status</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <inlineHelpText>Enter the tool tip here</inlineHelpText>
        <label>Tip</label>
        <length>131072</length>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>LongTextArea</type>
        <visibleLines>5</visibleLines>
    </fields>
    <fields>
        <fullName>Tool_Tip_Type__c</fullName>
        <description>Used to determine what the tool tip relates to</description>
        <externalId>false</externalId>
        <label>Tool Tip Type</label>
        <required>true</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Authorization</fullName>
                    <default>false</default>
                    <label>Authorization</label>
                </value>
                <value>
                    <fullName>Claim</fullName>
                    <default>false</default>
                    <label>Claim</label>
                </value>
                <value>
                    <fullName>Delegated</fullName>
                    <default>false</default>
                    <label>Delegated</label>
                </value>
                <value>
                    <fullName>Payment</fullName>
                    <default>false</default>
                    <label>Payment</label>
                </value>
                <value>
                    <fullName>Referral</fullName>
                    <default>false</default>
                    <label>Referral</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Value__c</fullName>
        <description>Used to hold the code the tool tip applies to</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <inlineHelpText>Enter the remark code or status tool tip applies to here.  Use &apos;*&apos; for the wildcard tool tip.</inlineHelpText>
        <label>Value</label>
        <length>255</length>
        <required>true</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
