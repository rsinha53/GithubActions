    <validationRules>
        <fullName>If_SOP_Link_Then_Name_Required</fullName>
        <active>true</active>
        <description>If the SOP or Document Link has a value but the SOP or Document Name field is blank then the user can&apos;t create the record.</description>
        <errorConditionFormula>AND( 
    NOT(ISBLANK(SOP_or_Document_Link__c)),
     ISBLANK(SOP_or_Document_Name__c))</errorConditionFormula>
        <errorDisplayField>SOP_or_Document_Name__c</errorDisplayField>
        <errorMessage>Complete this field.</errorMessage>
    </validationRules>
    <validationRules>
