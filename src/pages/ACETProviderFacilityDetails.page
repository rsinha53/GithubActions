                <apex:pageBlockTable value="{!agreements}" var="r" rendered="{!(ISNULL(agreements) || agreements.size == 0)}" columnsWidth="1%">
                    <apex:column style="width:50%" headerValue="{!FindAgreementsMessage}" styleClass="headerResultStyle"/>
