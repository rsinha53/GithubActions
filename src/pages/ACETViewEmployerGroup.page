       function initGroupAffiliationstable(grpAffiliationsinfolistStr){
           console.log('----inside  initGroupAffiliationstable----' + grpAffiliationsinfolistStr);
            if(grpAffiliationsinfolistStr==undefined || grpAffiliationsinfolistStr == null || grpAffiliationsinfolistStr=='') return;
           	grpAffiliationsinfolistStr = grpAffiliationsinfolistStr.replace(/&/g,'&amp;').replace(/\\"/g,'&quot;').replace(/</g, '&lt;').replace(/>/g, '&gt;');
            var grpAffiliationsinfolist = JSON.parse(grpAffiliationsinfolistStr)
            $('[id$="groupaffiliationsTable"]').DataTable({
                "data":grpAffiliationsinfolist,
                "columns":[
                    {
                        "data":"childGroupid",
                        "render": function(data, type, row, meta){
                            if(type === 'display'){
                            data = '<a onClick="navigatetogroupsearch('+data+')">' + data + '</a>';
                            }
                         return data;
                        }
                    },
                    {
                        "data":"childGroupname",
                        "render": function(data, type, row, meta){
                            if(type === 'display'){
                            data = '<a onClick="navigatetogroupsearch('+row.childGroupid+')">' + data + '</a>';
                            }
