<?xml version="1.0" encoding="UTF-8"?>
<Prompt xmlns="http://soap.sforce.com/2006/04/metadata">
    <masterLabel>New Case Saving</masterLabel>
    <promptVersions>
        <body>&lt;p&gt;Improved case saving now available! You are now only required to save a single case that will capture all information auto doc&apos;d for all topics selected during your call.&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;Please take a moment to complete the Learnsource Case Save Spire Desktop 362854-611131 that was assigned. Thank you.&amp;nbsp;&amp;nbsp;&lt;/p&gt;</body>
        <customApplication>SAE_Service_Console</customApplication>
        <delayDays>1</delayDays>
        <description>Prompt to advise Spire users of new case saving method</description>
        <displayType>DockedComposer</displayType>
        <endDate>2021-07-06</endDate>
        <header>Important Announcement!</header>
        <isPublished>true</isPublished>
        <masterLabel>New Case Saving</masterLabel>
        <publishedDate>2022-04-21</publishedDate>
        <shouldDisplayActionButton>false</shouldDisplayActionButton>
        <shouldIgnoreGlobalDelay>false</shouldIgnoreGlobalDelay>
        <startDate>2021-06-23</startDate>
        <targetPageKey1>Explore</targetPageKey1>
        <targetPageType>standard__navItemPage</targetPageType>
        <timesToDisplay>3</timesToDisplay>
        <title>New and Improved Case Saving!</title>
        <uiFormulaRule>
            <booleanFilter>1 OR 2</booleanFilter>
            <criteria>
                <leftValue>{!$User.Profile.Key}</leftValue>
                <operator>EQUAL</operator>
                <rightValue>custom_provider service</rightValue>
            </criteria>
            <criteria>
                <leftValue>{!$User.Profile.Key}</leftValue>
                <operator>EQUAL</operator>
                <rightValue>Admin</rightValue>
            </criteria>
        </uiFormulaRule>
        <userAccess>Everyone</userAccess>
        <userProfileAccess>SpecificProfiles</userProfileAccess>
        <versionNumber>1</versionNumber>
    </promptVersions>
</Prompt>
